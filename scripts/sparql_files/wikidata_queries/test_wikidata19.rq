#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?ed ?edLabel ?auteurLabel (year(?dateCreation) as ?AnneeCreation) ?lieuLabel ?coord ?lien
WHERE
{
  ?item wdt:P136 wd:Q1164267 .   
  ?item  wdt:P31 wd:Q571 .       
  OPTIONAL{
    ?item wdt:P50 ?auteur .      
  }
  OPTIONAL{
    ?item wdt:P571 ?dateCreation. 
  }
 
  ?item  wdt:P840 ?lieu .        
  MINUS {?item  wdt:P840 wd:Q933}
  ?lieu wdt:P625 ?coord .        
 
  {
    ?item wdt:P953 ?lien .      
    ?item wdt:P407 wd:Q150        
  }UNION{
    ?ed wdt:P629 ?item .          
    ?ed wdt:P953 ?lien .          
    ?ed wdt:P407 wd:Q150          
  }

  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],fr". }
} ORDER BY ?dateCreation  LIMIT 100
