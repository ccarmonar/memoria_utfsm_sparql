SELECT __ro2sq ( "s_17_10_t1"."O") AS "description",
  __ro2sq ( "s_17_10_t2"."O") AS "date",
  __ro2sq ( "s_17_10_t3"."O") AS "maxAmount"
FROM (
   SELECT  MAX (
         "s_12_2_t0"."O") AS "maxAmount"
    FROM DB.DBA.RDF_QUAD AS "s_12_2_t0"
    WHERE
      "s_12_2_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#amount' , 1))
OPTION (QUIETCAST)
    ) AS "s_12_3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_17_10_t1"
  ON (1)
  INNER JOIN DB.DBA.RDF_QUAD AS "s_17_10_t2"
  ON (
    "s_17_10_t2"."S" = "s_17_10_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_17_10_t3"
  ON (
    "s_17_10_t3"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( "s_12_3"."maxAmount")
    AND 
    "s_17_10_t3"."S" = "s_17_10_t2"."S"
    AND 
    "s_17_10_t3"."S" = "s_17_10_t1"."S")
WHERE
  "s_17_10_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#description' , 1))
  AND 
  "s_17_10_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#date' , 1))
  AND 
  "s_17_10_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#amount' , 1))
OPTION (QUIETCAST)
