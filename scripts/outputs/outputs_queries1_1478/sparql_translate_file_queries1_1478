 SELECT  __ro2sq ("s_37_2_rbc"."var2") AS "var2" FROM (SELECT  COUNT (
       "s_37_2_t0"."P") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q224494' , 1))
  GROUP BY "s_37_2_t0"."P"
OPTION (QUIETCAST)) AS "s_37_2_rbc"
