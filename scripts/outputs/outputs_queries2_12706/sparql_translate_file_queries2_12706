SELECT NULL /* due to conflict on var1 */ AS "var1",
  __id2in ( "s_37_5_t1"."S") AS "var2",
  __ro2sq ( "s_37_4"."var3") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
  LEFT OUTER JOIN (
    SELECT "s_37_4_t0"."S" AS "var2",
      "s_37_4_t0"."O" AS "var3"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P127' , 1))) AS "s_37_4"
  ON (
    "s_37_5_t1"."S" = "s_37_4"."var2")
WHERE
  "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_5_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q11303' , 1))
OPTION (QUIETCAST)
