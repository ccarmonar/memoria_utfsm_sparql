    AND 
    "s_37_15_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1040' , 1))
    AND 
   ( "s_37_12"."_::trans_obj_37_6" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q515' , 1)))
    AND 
   (((((((((( 'de'  =  NULL) OR 
