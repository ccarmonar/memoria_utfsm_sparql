SELECT __id2in ( "s_37_14_t3"."S") AS "var1",
  __id2in ( "s_37_13"."var2") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_14_t3"
  LEFT OUTER JOIN (
    SELECT "s_37_13_t2"."S" AS "var1",
      "s_37_13_t2"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_13_t2"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t0"
      ON (
        "s_37_13_t2"."O" = "s_37_7_t0"."S")
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t1"
      ON (
        "s_37_11_t1"."S" = "s_37_7_t0"."O")
    WHERE
      "s_37_13_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P971' , 1))
      AND 
      "s_37_7_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_11_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
      AND 
      "s_37_11_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q82794' , 1))) AS "s_37_13"
  ON (
    "s_37_14_t3"."S" = "s_37_13"."var1")
WHERE
  "s_37_14_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P971' , 1))
  AND 
  "s_37_14_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q31192219' , 1))
OPTION (QUIETCAST)
