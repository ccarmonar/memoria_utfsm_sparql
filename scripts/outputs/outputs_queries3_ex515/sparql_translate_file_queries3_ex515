SELECT __id2in ( "s_10_4_t0"."S") AS "person",
  __ro2sq ( "s_10_4_t1"."O") AS "first"
FROM DB.DBA.RDF_QUAD AS "s_10_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_10_4_t1"
  ON (
    "s_10_4_t0"."S" = "s_10_4_t1"."S")
WHERE
  "s_10_4_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#homeTel' , 1))
  AND 
  "s_10_4_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( '(229) 276-5135' )
  AND 
  "s_10_4_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
OPTION (QUIETCAST)
