SELECT __id2in ( "s_37_2_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P227' , 1))
  AND 
  (cast ( case ( NULL /* due to conflict on var3 */) when NULL then NULL when 0 then 'false' else 'true' end as varchar) like  '%string1%' )
OPTION (QUIETCAST)
