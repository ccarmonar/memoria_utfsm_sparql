SELECT TOP 10000 __ro2sq ( "s_37_6_t0"."O") AS "var1",
  __ro2sq ( "s_37_6_t1"."O") AS "var2",
  __id2in ( "s_37_6_t2"."P") AS "var3",
  __ro2sq ( "s_37_6_t2"."O") AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."O" = "s_37_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t1"."O" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
  AND 
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P21' , 1))
  AND 
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P461' , 1))
OPTION (QUIETCAST)
