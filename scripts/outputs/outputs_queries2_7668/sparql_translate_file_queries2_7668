SELECT __id2in ( "s_37_6_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t2"."S"
    AND 
    "s_37_6_t1"."S" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
  AND 
  "s_37_6_t0"."O" = __i2idn ( __bft( 'http://www.w3.org/2002/07/owl#Scientist' , 1))
  AND 
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2002/07/owl#country' , 1))
  AND 
  "s_37_6_t1"."O" = __i2idn ( __bft( 'http://es.dbpedia.org/resource/Peru' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_6_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q941045' , 1))
OPTION (QUIETCAST)
