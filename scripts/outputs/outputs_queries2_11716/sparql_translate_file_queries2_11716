SELECT __bft( 'http://www.wikidata.org/prop/P551' , 1) AS "var1",
  NULL /* due to conflict on var2Label */ AS "var2Label",
  __ro2sq ( "s_37_3_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P551' , 1))
OPTION (QUIETCAST)
