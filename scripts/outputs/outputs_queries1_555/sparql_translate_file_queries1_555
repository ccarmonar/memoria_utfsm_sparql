SELECT __id2in ( "s_37_2_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
  "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12418' , 1))
  AND 
  rdf_contains_impl ( "s_37_2_t0"."S",  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'wiki' ))
OPTION (QUIETCAST)
