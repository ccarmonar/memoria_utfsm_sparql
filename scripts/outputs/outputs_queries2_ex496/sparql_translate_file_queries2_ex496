SELECT __ro2sq ( "s_12_7_t0"."O") AS "description",
  __ro2sq ( "s_12_7_t1"."O") AS "date",
  __ro2sq ( "s_12_7_t2"."O") AS "amount"
FROM DB.DBA.RDF_QUAD AS "s_12_7_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_7_t1"
  ON (
    "s_12_7_t0"."S" = "s_12_7_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_7_t2"
  ON (
    "s_12_7_t0"."S" = "s_12_7_t2"."S"
    AND 
    "s_12_7_t1"."S" = "s_12_7_t2"."S")
WHERE
  "s_12_7_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#description' , 1))
  AND 
  (1 - isiri_id ( "s_12_7_t0"."O"))
  AND 
  "s_12_7_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#date' , 1))
  AND 
  "s_12_7_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#amount' , 1))
  AND 
  ( "s_12_7_t0"."O" in ( DB.DBA.RDF_OBJ_OF_SQLVAL ( 'lunch' ),  DB.DBA.RDF_OBJ_OF_SQLVAL ( 'dinner' )))
OPTION (QUIETCAST)
