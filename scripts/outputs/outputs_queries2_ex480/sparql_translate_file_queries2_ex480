 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://purl.org/dc/terms/dateCopyrighted' ),
         3,
         DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( '2013' ))),
      vector (
       "s_8_5_t1"."S"),
      vector (),
     1) AS "callret-0"
FROM DB.DBA.RDF_QUAD AS "s_8_5_t1"
WHERE
  "s_8_5_t1"."P" = __i2idn ( __bft( 'http://purl.org/dc/terms/dateCopyrighted' , 1))
  AND 
  "s_8_5_t1"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( '2013' )
OPTION (QUIETCAST)))
