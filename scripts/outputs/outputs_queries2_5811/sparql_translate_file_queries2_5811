SELECT TOP 10 __bft( 'http://www.wikidata.org/entity/Q221226' , 1) AS "var1",
  __ro2sq ( "s_37_7_t1"."O") AS "var2",
  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_7_t2"."O")) AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_7_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (1)
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t2"
  ON (1)
WHERE
  "s_37_7_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_7_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q13442814' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P577' , 1))
  AND 
  "s_37_7_t2"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
  AND 
  "s_37_7_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q221226' , 1))
  AND 
  "s_37_7_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q221226' , 1))
  AND 
  "s_37_7_t2"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q221226' , 1))
  AND 
  DB.DBA.RDF_LANGMATCHES ( __ro2sq ( "s_37_7_t2"."O"), 'en' )
OPTION (QUIETCAST)
