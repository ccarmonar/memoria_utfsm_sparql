 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var2") AS "var2" FROM (SELECT __ro2sq ( "s_37_4_t0"."O") AS "var1",
     COUNT (
       "s_37_4_t0"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."O" = "s_37_4_t1"."P")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
    AND 
    "s_37_4_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q13405496' , 1))
  GROUP BY "s_37_4_t0"."O"
OPTION (QUIETCAST)) AS "s_37_4_rbc"
