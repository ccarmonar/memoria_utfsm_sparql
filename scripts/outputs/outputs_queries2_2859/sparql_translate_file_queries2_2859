  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q62' , 1))
    AND 
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P47' , 1))
