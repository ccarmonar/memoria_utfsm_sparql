 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         2,
         0,
         3,
         __i2id ( UNAME'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' ),
         3,
         __i2id ( UNAME'http://purl.org/net/schemarama#Error' )),
        vector (
         2,
         0,
         3,
         __i2id ( UNAME'http://purl.org/net/schemarama#message' ),
         3,
         DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'location value should be a URI' )),
        vector (
         2,
         0,
         3,
         __i2id ( UNAME'http://purl.org/net/schemarama#implicated' ),
         1,
         0)),
      vector (
       "s_18_10_t3"."S"),
      vector (),
     1) AS "callret-0"
FROM DB.DBA.RDF_QUAD AS "s_18_10_t3"
WHERE
  "s_18_10_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/demo#location' , 1))
  AND 
  not ( is_named_iri_id ( "s_18_10_t3"."O"))
OPTION (QUIETCAST)))
