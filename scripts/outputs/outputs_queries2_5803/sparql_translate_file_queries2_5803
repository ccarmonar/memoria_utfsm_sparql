 SELECT  __ro2sq ("s_37_12_rbc"."var2") AS "var2", __ro2sq ("s_37_12_rbc"."var4") AS "var4", __ro2sq ("s_37_12_rbc"."var6") AS "var6", __ro2sq ("s_37_12_rbc"."var8") AS "var8" FROM (SELECT TOP 10000 __id2in ( "s_37_13_t7"."S") AS "var2",
     DB.DBA.GROUP_CONCAT_DISTINCT (
       __ro2sq ( "s_37_13_t6"."O")) AS "var4",
     DB.DBA.GROUP_CONCAT_DISTINCT (
       __ro2sq ( "s_37_8"."var5")) AS "var6",
     DB.DBA.GROUP_CONCAT_DISTINCT (
       __ro2sq ( "s_37_11"."var7")) AS "var8"
  FROM DB.DBA.RDF_QUAD AS "s_37_13_t5"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t6"
    ON (
      "s_37_13_t6"."O" = "s_37_13_t5"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t7"
    ON (
      "s_37_13_t7"."S" = "s_37_13_t6"."S")
    LEFT OUTER JOIN (
      SELECT "s_37_8_t0"."S" AS "var3",
        "s_37_8_t0"."O" AS "var5"
      FROM DB.DBA.RDF_QUAD AS "s_37_8_t0"
      WHERE
        "s_37_8_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P494' , 1))) AS "s_37_8"
    ON (
      "s_37_13_t6"."O" = "s_37_8"."var3"
      AND 
      "s_37_13_t5"."S" = "s_37_8"."var3")
    LEFT OUTER JOIN (
      SELECT "s_37_11_t1"."S" AS "var3",
        "s_37_11_t1"."O" AS "var7"
      FROM DB.DBA.RDF_QUAD AS "s_37_11_t1"
      WHERE
        "s_37_11_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2892' , 1))) AS "s_37_11"
    ON (
      "s_37_13_t6"."O" = "s_37_11"."var3"
      AND 
      "s_37_13_t5"."S" = "s_37_11"."var3")
  WHERE
    "s_37_13_t5"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_13_t5"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12136' , 1))
    AND 
    "s_37_13_t6"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_13_t7"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
    AND 
    "s_37_13_t7"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'en' )
  GROUP BY "s_37_13_t7"."S"
OPTION (QUIETCAST)) AS "s_37_12_rbc"
