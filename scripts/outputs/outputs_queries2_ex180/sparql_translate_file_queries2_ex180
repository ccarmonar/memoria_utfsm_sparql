 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://learningsparql.com/ns/addressbook#courseTitle' ),
         1,
         1)),
      vector (
       "s_12_4_t1"."S",
       __ro2lo ( "s_12_4_t1"."O")),
      vector (),
     1) AS "callret-0"
FROM DB.DBA.RDF_QUAD AS "s_12_4_t1"
WHERE
  "s_12_4_t1"."G" = __i2idn ( __bft( 'ex125.ttl' , 1))
  AND 
  "s_12_4_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#courseTitle' , 1))
  AND 
  1
OPTION (QUIETCAST)))
