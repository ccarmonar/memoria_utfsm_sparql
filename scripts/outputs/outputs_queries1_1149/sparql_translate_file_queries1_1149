SELECT __ro2sq ( "s_37_6_t1"."O") AS "var1",
  __id2in ( "s_37_6_t1"."S") AS "var2"
FROM (
   SELECT "s_37_2_t0"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
    WHERE
      "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q23173209' , 1))
    GROUP BY "s_37_2_t0"."S"
OPTION (QUIETCAST)
    ) AS "s_37_3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t1"."O" = "s_37_3"."var1")
WHERE
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/qualifier/P459' , 1))
  AND 
  isiri_id ( "s_37_6_t1"."O")
OPTION (QUIETCAST)
