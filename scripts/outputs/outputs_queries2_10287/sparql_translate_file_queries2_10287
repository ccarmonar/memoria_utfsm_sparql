SELECT TOP 100 __id2in ( "s_37_6_t0"."S") AS "var1",
  __id2in ( "s_37_6_t0"."P") AS "var2",
  __ro2sq ( "s_37_6_t0"."O") AS "var3",
  __id2in ( "s_37_6_t1"."S") AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (1)
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t0"."P" = "s_37_6_t2"."O"
    AND 
    "s_37_6_t1"."S" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P1921' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
OPTION (QUIETCAST)
