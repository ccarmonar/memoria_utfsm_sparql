 SELECT  "s_37_2_rbc"."var1" AS "var1" FROM (SELECT DISTINCT TOP 100 NULL /* due to conflict on var1 */ AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P854' , 1))
    AND 
    "s_37_2_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'string1' )
OPTION (QUIETCAST)) AS "s_37_2_rbc"
