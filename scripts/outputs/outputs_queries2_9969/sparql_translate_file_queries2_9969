SELECT TOP 100 __id2in ( "s_37_4_t0"."S") AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2",
  __ro2sq ( "s_37_4_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
  AND 
  "s_37_4_t1"."O" = __i2idn ( __bft( 'http://www.w3.org/2002/07/owl#Class' , 1))
OPTION (QUIETCAST)
