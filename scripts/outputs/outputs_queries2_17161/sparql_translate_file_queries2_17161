SELECT TOP 10 __id2in ( "s_37_2_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4167836' , 1))
  AND 
  rdf_contains_impl ( rdf_lcase_impl ( NULL /* due to conflict on var2 */),  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'France' ))
OPTION (QUIETCAST)
