SELECT TOP 1 0 AS "var2"
FROM (
   SELECT "s_37_4_t0"."S" AS "var3",
      rdf_year_impl ( "s_37_4_t1"."O") AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
      ON (
        "s_37_4_t0"."S" = "s_37_4_t1"."S")
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q17500081' , 1))
      AND 
      "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P580' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_5"
OPTION (QUIETCAST)
