SELECT __id2in ( "s_37_8"."var1") AS "var1",
  __ro2sq ( "s_37_8"."var2") AS "var2",
  __ro2sq ( "s_37_6"."var3") AS "var3"
FROM (
    SELECT "s_37_8_t3"."S" AS "var1",
      "s_37_8_t4"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
      ON (
        "s_37_8_t3"."S" = "s_37_8_t4"."S")
    WHERE
      "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_8_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q179700' , 1))
      AND 
      "s_37_8_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P571' , 1))
      AND 
     ( "s_37_8_t4"."O" <=  CAST ('0100-01-01T00:00:00' AS DATETIME))) AS "s_37_8"
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var1",
      "s_37_6_t0"."O" AS "var3"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P18' , 1))) AS "s_37_6"
  ON (
    "s_37_8"."var1" = "s_37_6"."var1")
OPTION (QUIETCAST)
