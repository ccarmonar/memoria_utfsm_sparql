 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var3") AS "var3" FROM (SELECT __ro2sq ( "s_37_4_t0"."O") AS "var1",
     COUNT (
       "s_37_4_t1"."O") AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2002' , 1))
  GROUP BY "s_37_4_t0"."O"
OPTION (QUIETCAST)) AS "s_37_4_rbc"
