SELECT __bft( 'http://www.wikidata.org/entity/Q21144338' , 1) AS "var1",
  __ro2sq ( "s_37_3_t0"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P356' , 1))
  AND 
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q21144338' , 1))
OPTION (QUIETCAST)
