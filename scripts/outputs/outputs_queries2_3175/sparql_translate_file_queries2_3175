 SELECT  "s_37_3_rbc"."var1" AS "var1", __ro2sq ("s_37_3_rbc"."var2") AS "var2", __ro2sq ("s_37_3_rbc"."var3") AS "var3" FROM (SELECT DISTINCT __bft( 'http://www.wikidata.org/entity/Q41749' , 1) AS "var1",
    "s_37_3_t0"."P" AS "var2",
    "s_37_3_t0"."O" AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  WHERE
    "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q41749' , 1))
OPTION (QUIETCAST)) AS "s_37_3_rbc"
