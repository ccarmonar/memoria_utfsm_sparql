SELECT __ro2sq ( "s_9_2_t0"."O") AS "email"
FROM DB.DBA.RDF_QUAD AS "s_9_2_t0"
WHERE
  "s_9_2_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
  AND 
  ( "s_9_2_t0"."G" in ( __i2idn ( __bft ( __bft( 'ex069.ttl' , 1), 1)),  __i2idn ( __bft ( __bft( 'ex122.ttl' , 1), 1))))
OPTION (QUIETCAST)
