SELECT __id2in ( "s_37_8"."var1") AS "var1",
  __ro2sq ( "s_37_8"."var2") AS "var2"
FROM (
    SELECT "s_37_2_t0"."O" AS "var2" ANY,
      NULL AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
    WHERE
      "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
      AND 
      "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P18' , 1))
    UNION ALL SELECT "s_37_7_t2"."O" AS "var2",
      "s_37_7_t2"."P" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_7_t1"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t2"
      ON (
        "s_37_7_t1"."O" = "s_37_7_t2"."S")
    WHERE
      "s_37_7_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
      AND 
      "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P18' , 1))) AS "s_37_8"
OPTION (QUIETCAST)
