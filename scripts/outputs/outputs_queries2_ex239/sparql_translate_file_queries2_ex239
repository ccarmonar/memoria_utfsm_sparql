SELECT __ro2sq ( "s_13_9"."firstName") AS "firstName",
  __ro2sq ( "s_13_9"."last") AS "last"
FROM (
   SELECT "s_13_8_t4"."S" AS "s",
      "s_13_8_t3"."O" AS "last",
      "s_13_8_t4"."O" AS "first",
      "s_12_6"."nickname" AS "nickname",
      coalesce ( "s_12_6"."nickname", "s_13_8_t4"."O") AS "firstName"
    FROM DB.DBA.RDF_QUAD AS "s_13_8_t3"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_13_8_t4"
      ON (
        "s_13_8_t4"."S" = "s_13_8_t3"."S")
      LEFT OUTER JOIN (
        SELECT "s_12_6_t0"."S" AS "s",
          "s_12_6_t0"."O" AS "nickname"
        FROM DB.DBA.RDF_QUAD AS "s_12_6_t0"
        WHERE
          "s_12_6_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#nick' , 1))) AS "s_12_6"
      ON (
        "s_13_8_t4"."S" = "s_12_6"."s"
        AND 
        "s_13_8_t3"."S" = "s_12_6"."s")
    WHERE
      "s_13_8_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
      AND 
      "s_13_8_t4"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
OPTION (QUIETCAST)
    ) AS "s_13_9"
OPTION (QUIETCAST)
