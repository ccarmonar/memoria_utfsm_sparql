SELECT __ro2sq ( "s_6_1_t0"."O") AS "craigEmail"
FROM DB.DBA.RDF_QUAD AS "s_6_1_t0"
WHERE
  "s_6_1_t0"."G" = __i2idn ( __bft( 'ex002.ttl' , 1))
  AND 
  "s_6_1_t0"."S" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#craig' , 1))
  AND 
  "s_6_1_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
OPTION (QUIETCAST)
