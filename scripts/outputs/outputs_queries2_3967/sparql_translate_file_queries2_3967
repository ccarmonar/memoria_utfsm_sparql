SELECT __id2in ( "s_37_6_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."O" = "s_37_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t1"."O" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/03/wn/wn20/schema/containsWordSense' , 1))
  AND 
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/03/wn/wn20/schema/word' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/03/wn/wn20/schema/lexicalForm' , 1))
  AND 
  "s_37_6_t2"."O" = DB.DBA.RDF_MAKE_OBJ_OF_TYPEDSQLVAL ( 'coffee' , __i2id( NULL),  'en-US' )
OPTION (QUIETCAST)
