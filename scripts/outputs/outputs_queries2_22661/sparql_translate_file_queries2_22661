SELECT __id2in ( "s_37_8"."var1") AS "var1",
  __ro2sq ( "s_37_8"."var2") AS "var2"
FROM (
    SELECT "s_37_8_t3"."S" AS "var1",
      "s_37_8_t4"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
      ON (
        "s_37_8_t3"."S" = "s_37_8_t4"."S")
    WHERE
      "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_8_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q11424' , 1))
      AND 
      "s_37_8_t4"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
      AND 
      rdf_contains_impl ( __ro2lo ( "s_37_8_t4"."O"),  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'string1' ))) AS "s_37_8"
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var1",
      "s_37_6_t0"."O" AS "var3"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
      AND 
     ( 'string2'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_6_t0"."O")))) AS "s_37_6"
  ON (
    "s_37_8"."var1" = "s_37_6"."var1")
WHERE
  not ( ( "s_37_6"."var3" is not null))
OPTION (QUIETCAST)
