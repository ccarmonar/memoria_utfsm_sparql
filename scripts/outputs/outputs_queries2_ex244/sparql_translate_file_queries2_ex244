SELECT __ro2sq ( "s_10_3"."o") AS "o",
  "s_10_3"."datatype" AS "datatype"
FROM (
   SELECT "s_10_2_t0"."S" AS "s",
      "s_10_2_t0"."P" AS "p",
      "s_10_2_t0"."O" AS "o",
      DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_10_2_t0"."O")) AS "datatype"
    FROM DB.DBA.RDF_QUAD AS "s_10_2_t0"
OPTION (QUIETCAST)
    ) AS "s_10_3"
OPTION (QUIETCAST)
