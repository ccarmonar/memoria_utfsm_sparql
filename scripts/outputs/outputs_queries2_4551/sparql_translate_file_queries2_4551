SELECT __id2in ( "s_37_4_t1"."S") AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."O"
    AND 
    "s_37_4_t0"."P" = "s_37_4_t1"."P")
WHERE
  "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q216047' , 1))
OPTION (QUIETCAST)
