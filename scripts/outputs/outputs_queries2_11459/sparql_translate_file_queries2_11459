SELECT __id2in ( "s_37_13_t2"."S") AS "var1",
  __ro2sq ( "s_37_13_t2"."O") AS "var2",
  __ro2sq ( "s_37_13_t3"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t2"
  ON (
    "s_37_13_t2"."S" = "s_37_3_t0"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t3"
  ON (
    "s_37_13_t2"."S" = "s_37_13_t3"."S"
    AND 
    "s_37_13_t3"."S" = "s_37_3_t0"."S")
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#wikiGroup' , 1))
  AND 
  "s_37_7_t1"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'wiktionary' )
  AND 
  "s_37_13_t2"."P" = __i2idn ( __bft( 'http://schema.org/name' , 1))
  AND 
  "s_37_13_t3"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
  not ( (cast ( __rdf_strsqlval ( "s_37_13_t2"."O", 0) as varchar) like  '%:%' ))
  AND 
 ( "s_37_13_t3"."O" <>  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/entity/Q5296' , 1), 1)))
OPTION (QUIETCAST)
