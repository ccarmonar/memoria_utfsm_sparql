SELECT __id2in ( "s_37_10"."var1") AS "var1",
  NULL /* due to conflict on var1Label */ AS "var1Label",
  __ro2sq ( "s_37_10"."var2") AS "var2",
  __ro2sq ( "s_37_10"."var3") AS "var3"
FROM (
    SELECT "s_37_4_t0-u12"."S" AS "var1",
      "s_37_4_t1-u12"."O" AS "var2" ANY,
      NULL AS "var3" ANY
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0-u12"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1-u12"
      ON (
        "s_37_4_t0-u12"."S" = "s_37_4_t1-u12"."S")
    WHERE
      "s_37_4_t0-u12"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_4_t0-u12"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4830453' , 1))
      AND 
      "s_37_4_t1-u12"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2002' , 1))
    UNION ALL SELECT "s_37_9_t2"."S" AS "var1",
      NULL AS "var2",
      "s_37_9_t3"."O" AS "var3"
    FROM DB.DBA.RDF_QUAD AS "s_37_9_t2"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_9_t3"
      ON (
        "s_37_9_t2"."S" = "s_37_9_t3"."S")
    WHERE
      "s_37_9_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_9_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4830453' , 1))
      AND 
      "s_37_9_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P856' , 1))) AS "s_37_10"
OPTION (QUIETCAST)
