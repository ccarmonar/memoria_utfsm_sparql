SELECT __id2in ( "s_37_3_t0"."S") AS "var1",
  __id2in ( "s_37_11_t2"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t2"
  ON (
    "s_37_11_t2"."S" = "s_37_7_t1"."O")
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
  AND 
  "s_37_11_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_11_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q10876391' , 1))
OPTION (QUIETCAST)
