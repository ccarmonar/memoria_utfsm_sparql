SELECT __bft( 'http://www.wikidata.org/entity/Q188670' , 1) AS "var1",
  __id2in ( "s_37_6_t0"."P") AS "var2",
  NULL /* due to conflict on var3 */ AS "var3",
  __id2in ( "s_37_6_t1"."S") AS "var2Label",
  __bft( 'http://www.wikidata.org/entity/Q188993' , 1) AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."P" = "s_37_6_t1"."O")
WHERE
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
  AND 
  "s_37_6_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q188670' , 1))
  AND 
  "s_37_6_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q188993' , 1))
OPTION (QUIETCAST)
