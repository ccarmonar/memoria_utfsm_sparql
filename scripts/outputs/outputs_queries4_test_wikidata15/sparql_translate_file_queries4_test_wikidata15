 SELECT  __ro2sq ("s_47_14_rbc"."item") AS "item", __ro2sq ("s_47_14_rbc"."LabelEN") AS "LabelEN", __ro2sq ("s_47_14_rbc"."LabelUK") AS "LabelUK", __ro2sq ("s_47_14_rbc"."page_titleRO") AS "page_titleRO" FROM (SELECT DISTINCT TOP 300 "s_47_14_t0"."S" AS "item",
    "s_47_14_t5"."O" AS "LabelEN",
    "s_47_14_t6"."O" AS "LabelUK",
    "s_47_14_t4"."O" AS "page_titleRO"
  FROM DB.DBA.RDF_QUAD AS "s_47_14_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t1"
    ON (
      "s_47_14_t0"."S" = "s_47_14_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t2"
    ON (
      "s_47_14_t0"."S" = "s_47_14_t2"."O"
      AND 
      "s_47_14_t1"."S" = "s_47_14_t2"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t3"
    ON (
      "s_47_14_t2"."S" = "s_47_14_t3"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t4"
    ON (
      "s_47_14_t2"."S" = "s_47_14_t4"."S"
      AND 
      "s_47_14_t3"."S" = "s_47_14_t4"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t5"
    ON (
      "s_47_14_t0"."S" = "s_47_14_t5"."S"
      AND 
      "s_47_14_t1"."S" = "s_47_14_t5"."S"
      AND 
      "s_47_14_t2"."O" = "s_47_14_t5"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_47_14_t6"
    ON (
      "s_47_14_t0"."S" = "s_47_14_t6"."S"
      AND 
      "s_47_14_t1"."S" = "s_47_14_t6"."S"
      AND 
      "s_47_14_t2"."O" = "s_47_14_t6"."S"
      AND 
      "s_47_14_t5"."S" = "s_47_14_t6"."S")
  WHERE
    "s_47_14_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_47_14_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q532' , 1))
    AND 
    "s_47_14_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P17' , 1))
    AND 
    "s_47_14_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q212' , 1))
    AND 
    "s_47_14_t2"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_47_14_t3"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
    AND 
    "s_47_14_t3"."O" = __i2idn ( __bft( 'https://ro.wikipedia.org/' , 1))
    AND 
    "s_47_14_t4"."P" = __i2idn ( __bft( 'http://schema.org/name' , 1))
    AND 
    "s_47_14_t5"."P" = __i2idn ( __bft( 'http://www.w3.org/2000/01/rdf-schema#label' , 1))
    AND 
    "s_47_14_t6"."P" = __i2idn ( __bft( 'http://www.w3.org/2000/01/rdf-schema#label' , 1))
    AND 
   ( 'en'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_47_14_t5"."O")))
    AND 
   ( 'uk'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_47_14_t6"."O")))
OPTION (QUIETCAST)) AS "s_47_14_rbc"
