SELECT __id2in ( "s_14_4_t1"."S") AS "graph",
  __ro2sq ( "s_13_2_t0"."O") AS "email"
FROM DB.DBA.RDF_QUAD AS "s_14_4_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_13_2_t0"
  ON (
    "s_14_4_t1"."S" = "s_13_2_t0"."G")
WHERE
  "s_14_4_t1"."G" = __i2idn ( __bft( 'ex134.ttl' , 1))
  AND 
  "s_14_4_t1"."P" = __i2idn ( __bft( 'http://purl.org/dc/elements/1.1/date' , 1))
  AND 
  "s_14_4_t1"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( '2011-09-24' )
  AND 
  "s_13_2_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
  AND 
  ( "s_14_4_t1"."S" in ( __i2idn ( __bft ( __bft( 'ex125.ttl' , 1), 1)),  __i2idn ( __bft ( __bft( 'ex122.ttl' , 1), 1))))
OPTION (QUIETCAST)
