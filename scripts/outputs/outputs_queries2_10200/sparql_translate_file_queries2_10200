SELECT TOP 1 0 AS "var2"
FROM (
   SELECT "s_37_2_t0"."S" AS "var3",
      NULL /* due to conflict on var3Label */ AS "var3Label"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
    WHERE
      "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q13100073' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_3"
OPTION (QUIETCAST)
