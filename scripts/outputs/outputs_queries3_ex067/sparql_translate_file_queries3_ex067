SELECT __ro2sq ( "s_12_8_t1"."O") AS "first",
  __ro2sq ( "s_12_8_t2"."O") AS "last"
FROM DB.DBA.RDF_QUAD AS "s_12_8_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_8_t2"
  ON (
    "s_12_8_t1"."S" = "s_12_8_t2"."S")
WHERE
  "s_12_8_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
  AND 
  "s_12_8_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
  AND 
  not ( EXISTS ( (
     SELECT TOP 1 1 AS __ask_retval
      FROM DB.DBA.RDF_QUAD AS "s_12_6_t0"
      WHERE
        "s_12_6_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#workTel' , 1))
        AND 
        "s_12_6_t0"."S" = "s_12_8_t2"."S"
OPTION (QUIETCAST)
     )))
OPTION (QUIETCAST)
