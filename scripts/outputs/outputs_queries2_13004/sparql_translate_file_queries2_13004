SELECT __id2in ( "s_37_13_t1"."S") AS "var1"
FROM (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_4_t0"."S" AS "_::trans_subj_37_0",
      "s_37_4_t0"."O" AS "_::trans_obj_37_1"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_7"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_23_t3"
  ON (1)
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t1"
  ON (
    "s_37_13_t1"."S" = "s_37_7"."_::trans_subj_37_0")
  INNER JOIN (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_19_t2"."S" AS "_::trans_subj_37_15",
      "s_37_19_t2"."O" AS "_::trans_obj_37_16"
    FROM DB.DBA.RDF_QUAD AS "s_37_19_t2"
    WHERE
      "s_37_19_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_22"
  ON (
    "s_37_13_t1"."O" = "s_37_22"."_::trans_subj_37_15")
WHERE
  "s_37_23_t3"."S" = __i2idn ( __bft( 'http://www.bigdata.com/queryHints#Prior' , 1))
  AND 
  "s_37_23_t3"."P" = __i2idn ( __bft( 'http://www.bigdata.com/queryHints#gearing' , 1))
  AND 
  "s_37_23_t3"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'reverse' )
  AND 
  "s_37_13_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  isiri_id ( "s_37_13_t1"."O")
  AND 
 ( "s_37_7"."_::trans_obj_37_1" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1198' , 1)))
  AND 
 ( "s_37_22"."_::trans_obj_37_16" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q387917' , 1)))
  AND 
  1
  AND 
  1
OPTION (QUIETCAST)
