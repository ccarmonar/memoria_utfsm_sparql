 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1" FROM (SELECT DISTINCT TOP 5000, 5000 __id2in ( "s_37_4_t0"."S") AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P364' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P407' , 1))
  ORDER BY __id2in ( "s_37_4_t0"."S") ASC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
