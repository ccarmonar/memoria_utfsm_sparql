 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_6_t0"."P" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."O" = "s_37_6_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t2"."S")
  WHERE
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P854' , 1))
    AND 
    "s_37_6_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/value/P1082' , 1))
OPTION (QUIETCAST)) AS "s_37_6_rbc"
