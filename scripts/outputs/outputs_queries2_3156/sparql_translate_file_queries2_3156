SELECT TOP 10 __id2in ( "s_37_4_t0"."S") AS "var1",
  NULL /* due to conflict on var2 */ AS "var2",
  NULL /* due to conflict on var3Label */ AS "var3Label"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."O")
WHERE
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P703' , 1))
  AND 
  "s_37_4_t1"."S" = __i2idn ( __bft( 'https://en.wikipedia.org/wiki/Lung_cancer' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
OPTION (QUIETCAST)
