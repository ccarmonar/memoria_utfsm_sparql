 SELECT  __ro2sq ("s_37_2_rbc"."var1Label") AS "var1Label" FROM (SELECT DISTINCT "s_37_2_t0"."O" AS "var1Label"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/prop/2044' , 1))
    AND 
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#label' , 1))
    AND 
    DB.DBA.RDF_LANGMATCHES ( DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O")), 'ES' )
OPTION (QUIETCAST)) AS "s_37_2_rbc"
