 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_2_t0"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P570' , 1))
    AND 
   ( "s_37_2_t0"."O" >=  CAST ('1964-12-01T00:00:00Z' AS DATETIME))
    AND 
   ( "s_37_2_t0"."O" <=  CAST ('1964-12-31T23:59:59Z' AS DATETIME))
OPTION (QUIETCAST)) AS "s_37_2_rbc"
