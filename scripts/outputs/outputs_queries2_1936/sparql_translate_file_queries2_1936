SELECT __ro2sq ( "s_37_7_t1"."O") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q30' , 1))
  AND 
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P6' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P6' , 1))
OPTION (QUIETCAST)
