SELECT __ro2sq ( "s_37_4"."var1") AS "var1",
  __ro2sq ( "s_37_7"."var2") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_9_t3"
  LEFT OUTER JOIN (
    SELECT "s_37_4_t0"."S" AS "var3",
      "s_37_4_t0"."O" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2067' , 1))) AS "s_37_4"
  ON (
    "s_37_9_t3"."S" = "s_37_4"."var3")
  LEFT OUTER JOIN (
    SELECT "s_37_7_t1"."S" AS "var3",
      "s_37_7_t1"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_7_t1"
    WHERE
      "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2386' , 1))) AS "s_37_7"
  ON (
    "s_37_9_t3"."S" = "s_37_7"."var3")
WHERE
  "s_37_9_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_9_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q217526' , 1))
OPTION (QUIETCAST)
