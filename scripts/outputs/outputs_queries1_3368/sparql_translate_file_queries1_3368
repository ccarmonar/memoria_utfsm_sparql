 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", __ro2sq ("s_37_2_rbc"."var2") AS "var2" FROM (SELECT TOP 50 __id2in ( "s_37_2_t0"."S") AS "var1",
    __ro2sq ( "s_37_2_t0"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P356' , 1))
  ORDER BY rdf_rand_impl () ASC
OPTION (QUIETCAST)) AS "s_37_2_rbc"
