 SELECT  __ro2sq ("s_37_6_rbc"."var2") AS "var2", __ro2sq ("s_37_6_rbc"."var3") AS "var3", __ro2sq ("s_37_6_rbc"."var3Label") AS "var3Label", __ro2sq ("s_37_6_rbc"."var4") AS "var4" FROM (SELECT  COUNT (
       "s_37_6_t1"."S") AS "var2",
    __id2in ( "s_37_6_t0"."S") AS "var3",
    NULL /* due to conflict on var3Label */ AS "var3Label",
    __ro2sq ( "s_37_6_t0"."O") AS "var4"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t1"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t1"."S" = "s_37_6_t2"."S")
  WHERE
    "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P685' , 1))
    AND 
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P703' , 1))
    AND 
    "s_37_6_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_6_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q7187' , 1))
  GROUP BY "s_37_6_t0"."S", NULL /* due to conflict on var3Label */, "s_37_6_t0"."O"
OPTION (QUIETCAST)) AS "s_37_6_rbc"
