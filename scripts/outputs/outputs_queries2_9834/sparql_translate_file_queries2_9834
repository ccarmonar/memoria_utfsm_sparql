 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_6_t2"."P" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."P" = "s_37_6_t1"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t1"."S" = "s_37_6_t2"."S")
  WHERE
    "s_37_6_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q9197118' , 1))
    AND 
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2002/07/owl#onProperty' , 1))
OPTION (QUIETCAST)) AS "s_37_6_rbc"
