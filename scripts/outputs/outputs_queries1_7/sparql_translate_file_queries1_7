    "s_37_12_t1"."O" = __i2idn ( __bft( 'https://az.wikisource.org/' , 1))
    AND 
    "s_37_12_t2"."P" = __i2idn ( __bft( 'http://schema.org/name' , 1))
    AND 
    "s_37_12_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_12_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4167836' , 1))
    AND 
    "s_37_12_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P301' , 1))
