    "s_37_12_t3"."S" AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_12_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t1"
    ON (
      "s_37_12_t0"."S" = "s_37_12_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t2"
    ON (
      "s_37_12_t0"."S" = "s_37_12_t2"."S"
      AND 
      "s_37_12_t1"."S" = "s_37_12_t2"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t3"
    ON (1)
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t4"
    ON (
      "s_37_12_t0"."O" = "s_37_12_t4"."O"
      AND 
      "s_37_12_t3"."S" = "s_37_12_t4"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t5"
    ON (
      "s_37_12_t0"."O" = "s_37_12_t5"."S"
      AND 
      "s_37_12_t4"."O" = "s_37_12_t5"."S")
  WHERE
    "s_37_12_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_12_t1"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
