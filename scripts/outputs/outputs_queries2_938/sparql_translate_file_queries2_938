SELECT __bft( 'http://www.wikidata.org/entity/statement/Q42-29c1b057-497d-7d15-864e-3d889a76c750' , 1) AS "var1",
  __id2in ( "s_37_3_t0"."P") AS "var2",
  __ro2sq ( "s_37_3_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/statement/Q42-29c1b057-497d-7d15-864e-3d889a76c750' , 1))
OPTION (QUIETCAST)
