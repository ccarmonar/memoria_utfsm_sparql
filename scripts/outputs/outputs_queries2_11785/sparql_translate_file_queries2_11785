SELECT __id2in ( "s_37_7_t2"."S") AS "var1",
  __id2in ( "s_37_6"."var2") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_7_t2"
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var2",
      "s_37_6_t0"."O" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
      ON (
        "s_37_6_t0"."S" = "s_37_6_t1"."S")
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
      AND 
      isiri_id ( "s_37_6_t0"."O")
      AND 
      "s_37_6_t1"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
      AND 
      "s_37_6_t1"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'en' )
      AND 
     ( 'string1'  =  rdf_substr_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( __bft (__id2in ( "s_37_6_t0"."S"), 2)),  1,  25))) AS "s_37_6"
  ON (
    "s_37_7_t2"."S" = "s_37_6"."var1")
WHERE
  "s_37_7_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_7_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q52' , 1))
OPTION (QUIETCAST)
