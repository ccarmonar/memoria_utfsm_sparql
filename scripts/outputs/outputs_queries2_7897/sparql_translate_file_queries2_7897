SELECT DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O")) AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42196' , 1))
  AND 
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P685' , 1))
OPTION (QUIETCAST)
