 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", __ro2sq ("s_37_2_rbc"."var3") AS "var3" FROM (SELECT __ro2sq ( "s_37_2_t0"."O") AS "var1",
     COUNT ( DISTINCT
       "s_37_2_t0"."S") AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P496' , 1))
  GROUP BY "s_37_2_t0"."O"
OPTION (QUIETCAST)) AS "s_37_2_rbc"
