 SELECT  __ro2sq ("s_37_7_rbc"."var1") AS "var1", __ro2sq ("s_37_7_rbc"."var2") AS "var2", __ro2sq ("s_37_7_rbc"."var3") AS "var3", __ro2sq ("s_37_7_rbc"."var4") AS "var4" FROM (SELECT DISTINCT "s_37_6"."var1" AS "var1",
    "s_37_6"."var2" AS "var2",
    "s_37_6"."var3" AS "var3",
    "s_37_6"."var4" AS "var4"
  FROM (
      SELECT "s_37_2_t0-u8"."O" AS "var2" ANY,
        "s_37_2_t0-u8"."P" AS "var1",
        NULL AS "var4",
        NULL AS "var3"
      FROM DB.DBA.RDF_QUAD AS "s_37_2_t0-u8"
      WHERE
        "s_37_2_t0-u8"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q250089' , 1))
      UNION ALL SELECT NULL AS "var2",
        NULL AS "var1",
        "s_37_5_t1"."P" AS "var4",
        "s_37_5_t1"."S" AS "var3"
      FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
      WHERE
        "s_37_5_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q250089' , 1))) AS "s_37_6"
OPTION (QUIETCAST)) AS "s_37_7_rbc"
