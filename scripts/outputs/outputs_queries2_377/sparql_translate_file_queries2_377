SELECT TOP 10 __id2in ( "s_37_13_t2"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_13_t2"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t3"
  ON (
    "s_37_13_t2"."S" = "s_37_13_t3"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t0"
  ON (
    "s_37_13_t3"."O" = "s_37_7_t0"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t1"
  ON (
    "s_37_11_t1"."S" = "s_37_7_t0"."O")
WHERE
  "s_37_13_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_13_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q13442814' , 1))
  AND 
  "s_37_13_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_7_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
  AND 
  "s_37_11_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
  AND 
  "s_37_11_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q229883' , 1))
OPTION (QUIETCAST)
