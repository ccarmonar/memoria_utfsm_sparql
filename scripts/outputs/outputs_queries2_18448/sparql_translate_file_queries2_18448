SELECT __id2in ( "s_37_8_t3"."S") AS "var1",
  __id2in ( "s_37_8_t4"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
  ON (1)
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var1",
      "s_37_6_t0"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))
      AND 
      isiri_id ( "s_37_6_t0"."O")) AS "s_37_6"
  ON (
    "s_37_8_t3"."S" = "s_37_6"."var1"
    AND 
    "s_37_8_t4"."S" = "s_37_6"."var2")
WHERE
  "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P703' , 1))
  AND 
  "s_37_8_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q21397863' , 1))
  AND 
  "s_37_8_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_8_t4"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q139677' , 1))
OPTION (QUIETCAST)
