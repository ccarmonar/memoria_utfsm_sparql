SELECT TOP 50 __id2in ( "s_37_6_t0"."S") AS "var1",
  __id2in ( "s_37_6_t1"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t1"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t1"."S" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2013' , 1))
  AND 
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
  AND 
  "s_37_6_t2"."O" = __i2idn ( __bft( 'https://es.wikipedia.org/' , 1))
OPTION (QUIETCAST)
