 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", __ro2sq ("s_37_2_rbc"."var2") AS "var2" FROM (SELECT __id2in ( "s_37_2_t0"."P") AS "var1",
    __ro2sq ( "s_37_2_t0"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q9197118' , 1))
    AND 
    (cast ( __bft (__id2in ( "s_37_2_t0"."P"), 2) as varchar) like  '%string1%' )
  ORDER BY __id2in ( "s_37_2_t0"."P") ASC, __ro2sq ( "s_37_2_t0"."O") ASC
OPTION (QUIETCAST)) AS "s_37_2_rbc"
