      AND 
      "s_37_2_t0-c5-u19"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q11879590' , 1))
      AND 
      "s_37_14_t3-u19"."O" = DB.DBA.RDF_MAKE_OBJ_OF_TYPEDSQLVAL ( 'Marko' , __i2id( NULL),  'en' )
    UNION ALL SELECT "s_37_14_t4-u20"."S" AS "var1",
