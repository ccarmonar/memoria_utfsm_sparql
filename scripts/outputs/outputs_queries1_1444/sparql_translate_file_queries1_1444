 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1" FROM (SELECT DISTINCT TOP 100 "s_37_2_t0"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P854' , 1))
    AND 
   ( 'string1'  =  __rdf_strsqlval ( "s_37_2_t0"."O", 0))
OPTION (QUIETCAST)) AS "s_37_2_rbc"
