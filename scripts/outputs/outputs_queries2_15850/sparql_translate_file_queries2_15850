SELECT  COUNT ( DISTINCT
     "s_37_6"."var1") AS "var2"
FROM (
    SELECT "s_37_2_t0-u9"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0-u9"
    WHERE
      "s_37_2_t0-u9"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P17' , 1))
      AND 
      "s_37_2_t0-u9"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q20' , 1))
    UNION ALL SELECT "s_37_5_t1"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
    WHERE
      "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
      AND 
      "s_37_5_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q20' , 1))) AS "s_37_6"
OPTION (QUIETCAST)
