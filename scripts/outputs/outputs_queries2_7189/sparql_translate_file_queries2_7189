SELECT __ro2sq ( "s_37_4_t1"."O") AS "var1",
  NULL /* due to conflict on var2Label */ AS "var2Label",
  NULL /* due to conflict on var3 */ AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (1)
WHERE
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q2831' , 1))
  AND 
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P136' , 1))
  AND 
  "s_37_4_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q2831' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P21' , 1))
OPTION (QUIETCAST)
