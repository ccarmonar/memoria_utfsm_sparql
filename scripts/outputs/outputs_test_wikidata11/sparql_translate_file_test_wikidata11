 SELECT  __ro2sq ("s_45_13_rbc"."item") AS "item", __ro2sq ("s_45_13_rbc"."wikisourceSitelink") AS "wikisourceSitelink", __ro2sq ("s_45_13_rbc"."wikisourceLanguage") AS "wikisourceLanguage" FROM (SELECT DISTINCT TOP 300 __ro2sq ( "s_45_13_t3"."O") AS "item",
    __id2in ( "s_45_13_t1"."S") AS "wikisourceSitelink",
    __ro2sq ( "s_45_13_t2"."O") AS "wikisourceLanguage"
  FROM DB.DBA.RDF_QUAD AS "s_45_13_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_45_13_t1"
    ON (
      "s_45_13_t0"."S" = "s_45_13_t1"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_45_13_t2"
    ON (
      "s_45_13_t1"."S" = "s_45_13_t2"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_45_13_t3"
    ON (
      "s_45_13_t1"."S" = "s_45_13_t3"."S"
      AND 
      "s_45_13_t2"."S" = "s_45_13_t3"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_45_13_t4"
    ON (1)
    INNER JOIN DB.DBA.RDF_QUAD AS "s_45_13_t5"
    ON (
      "s_45_13_t3"."O" = "s_45_13_t5"."O"
      AND 
      "s_45_13_t4"."S" = "s_45_13_t5"."S")
  WHERE
    "s_45_13_t0"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#wikiGroup' , 1))
    AND 
    "s_45_13_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'wikisource' )
    AND 
    "s_45_13_t1"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
    AND 
    "s_45_13_t2"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
    AND 
    "s_45_13_t3"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_45_13_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_45_13_t4"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q13442814' , 1))
    AND 
    "s_45_13_t5"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P50' , 1))
  ORDER BY __ro2sq ( "s_45_13_t2"."O") ASC
OPTION (QUIETCAST)) AS "s_45_13_rbc"
