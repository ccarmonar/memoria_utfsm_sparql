 SELECT  __ro2sq ("s_37_8_rbc"."var1") AS "var1", __ro2sq ("s_37_8_rbc"."var2Label") AS "var2Label" FROM (SELECT DISTINCT "s_37_8_t3"."S" AS "var1",
    "s_37_8_t2"."O" AS "var2Label"
  FROM DB.DBA.RDF_QUAD AS "s_37_8_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
    ON (
      "s_37_8_t0"."S" = "s_37_8_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t2"
    ON (
      "s_37_8_t0"."S" = "s_37_8_t2"."S"
      AND 
      "s_37_8_t1"."S" = "s_37_8_t2"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t3"
    ON (1)
  WHERE
    "s_37_8_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_8_t1"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
    AND 
    "s_37_8_t1"."O" = __i2idn ( __bft( 'https://az.wikisource.org/' , 1))
    AND 
    "s_37_8_t2"."P" = __i2idn ( __bft( 'http://schema.org/name' , 1))
    AND 
    "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_8_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4167836' , 1))
    AND 
    rdf_regex_impl ( __ro2sq ( "s_37_8_t2"."O"),   charset_recode ('sur…ôsi' , 'UTF-8', '_WIDE_'),  'i' )
OPTION (QUIETCAST)) AS "s_37_8_rbc"
