 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1", __ro2sq ("s_37_6_rbc"."var3") AS "var3" FROM (SELECT __ro2sq ( "s_37_6_t2"."O") AS "var1",
     COUNT ( DISTINCT
       "s_37_6_t0"."S") AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t1"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t1"."S" = "s_37_6_t2"."S")
  WHERE
    "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P3360' , 1))
    AND 
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_6_t2"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
    AND 
   ( 'string1'  =  rdf_substr_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( __bft (__id2in ( "s_37_6_t1"."S"), 2)),  11,  15))
  GROUP BY "s_37_6_t2"."O"
  ORDER BY 2 DESC
OPTION (QUIETCAST)) AS "s_37_6_rbc"
