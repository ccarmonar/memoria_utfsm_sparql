 SELECT  __ro2sq ("s_37_11_rbc"."var1") AS "var1", __ro2sq ("s_37_11_rbc"."var2") AS "var2", __ro2sq ("s_37_11_rbc"."var3") AS "var3" FROM (SELECT __ro2sq ( "s_0_14"."var1") AS "var1",
    rdf_substr_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( __rdf_strsqlval ( "s_0_14"."var1", 0)),  32,  250) AS "var2",
     COUNT (
       "s_0_14"."var1") AS "var3"
  FROM (
      SELECT "s_37_11_t3"."O" AS "var1" ANY
      FROM DB.DBA.RDF_QUAD AS "s_37_4_t0-c5"
        INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1-c5"
        ON (
          "s_37_4_t1-c5"."S" = "s_37_4_t0-c5"."O")
        INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t3"
        ON (
          "s_37_11_t3"."S" = "s_37_4_t0-c5"."S")
      WHERE
        "s_37_4_t0-c5"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))
        AND 
        "s_37_4_t1-c5"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P4009' , 1))
        AND 
        "s_37_11_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
      UNION ALL SELECT "s_37_11_t3"."O" AS "var1"
      FROM DB.DBA.RDF_QUAD AS "s_37_7_t2-c11"
        INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t3"
        ON (
          "s_37_11_t3"."S" = "s_37_7_t2-c11"."S")
      WHERE
        "s_37_7_t2-c11"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P4009' , 1))
        AND 
        "s_37_11_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))) AS "s_0_14"
  GROUP BY "s_0_14"."var1", NULL /* due to conflict on var1Label */
  ORDER BY NULL /* due to conflict on var1Label */ ASC
OPTION (QUIETCAST)) AS "s_37_11_rbc"
