SELECT __bft( 'http://www.wikidata.org/entity/Q15940566' , 1) AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/Q751876' , 1))
  AND 
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q15940566' , 1))
OPTION (QUIETCAST)
