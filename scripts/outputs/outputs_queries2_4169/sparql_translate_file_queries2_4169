SELECT __id2in ( "s_37_4_t0"."P") AS "var1",
  __ro2sq ( "s_37_4_t0"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."O" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://dbpedia.org/resource/Mahatma_Gandhi' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_4_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q773087' , 1))
OPTION (QUIETCAST)
