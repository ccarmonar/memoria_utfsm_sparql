 SELECT  "s_37_4_rbc"."var2" AS "var2" FROM (SELECT DISTINCT  COUNT ( DISTINCT
       "s_37_4_t1"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q22988604' , 1))
    AND 
   ( 'en'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_4_t1"."O")))
OPTION (QUIETCAST)) AS "s_37_4_rbc"
