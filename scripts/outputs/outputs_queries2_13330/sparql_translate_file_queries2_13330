SELECT __id2in ( "s_37_8_t4"."S") AS "var1",
  __ro2sq ( "s_37_6"."var2") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
  ON (
    "s_37_8_t4"."S" = "s_37_8_t3"."S")
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var1",
      "s_37_6_t0"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
      AND 
     ( 'cs'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_6_t0"."O")))) AS "s_37_6"
  ON (
    "s_37_8_t4"."S" = "s_37_6"."var1"
    AND 
    "s_37_8_t3"."S" = "s_37_6"."var1")
WHERE
  "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_8_t3"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q928830' , 1))
  AND 
  "s_37_8_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))
  AND 
  "s_37_8_t4"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q190271' , 1))
OPTION (QUIETCAST)
