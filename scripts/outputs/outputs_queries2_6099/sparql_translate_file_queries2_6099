SELECT TOP 10 __bft( 'http://www.wikidata.org/entity/1492' , 1) AS "var1",
  __id2in ( "s_37_3_t0"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
  "s_37_3_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/1492' , 1))
OPTION (QUIETCAST)
