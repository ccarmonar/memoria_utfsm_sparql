SELECT __id2in ( "s_37_2_t0"."P") AS "var1",
  __ro2sq ( "s_37_2_t0"."O") AS "var2",
  coalesce ( (1 - isiri_id ( "s_37_2_t0"."O")), case when ( is_named_iri_id ( "s_37_2_t0"."O")) then ( 'IRI' ) else ( case when ( is_bnode_iri_id ( "s_37_2_t0"."O")) then ( 'blank node' ) else ( 'string1' ) end) end) AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12418' , 1))
OPTION (QUIETCAST)
