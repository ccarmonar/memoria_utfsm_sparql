SELECT __id2in ( "s_37_4_t0"."S") AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."P" = "s_37_4_t1"."O")
WHERE
  "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1333923' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
OPTION (QUIETCAST)
