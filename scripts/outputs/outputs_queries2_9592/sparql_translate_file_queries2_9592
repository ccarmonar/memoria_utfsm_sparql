SELECT __bft( 'http://www.wikidata.org/entity/Q9068' , 1) AS "var1",
  __id2in ( "s_37_5_t0"."P") AS "var2",
  __ro2sq ( "s_37_5_t0"."O") AS "var3",
  __ro2sq ( "s_37_5_t1"."O") AS "var4Label"
FROM DB.DBA.RDF_QUAD AS "s_37_5_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t1"
  ON (1)
WHERE
  "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P19' , 1))
  AND 
  "s_37_5_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q9068' , 1))
  AND 
  "s_37_5_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q9068' , 1))
OPTION (QUIETCAST)
