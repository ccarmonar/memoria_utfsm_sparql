SELECT __id2in ( "s_37_2_t0"."P") AS "var1",
  __ro2sq ( "s_37_2_t0"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1' , 1))
  AND 
 ( "s_37_2_t0"."O" >=  CAST ('1945-01-01T00:00:00Z' AS DATETIME))
  AND 
 ( "s_37_2_t0"."O" <=  CAST ('1946-01-01T00:00:00Z' AS DATETIME))
OPTION (QUIETCAST)
