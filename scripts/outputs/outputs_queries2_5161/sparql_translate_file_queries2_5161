 SELECT  __ro2sq ("s_37_10_rbc"."var1") AS "var1", __ro2sq ("s_37_10_rbc"."var2") AS "var2" FROM (SELECT DISTINCT "s_37_10_t0"."S" AS "var1",
    "s_37_10_t2"."S" AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_10_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t1"
    ON (
      "s_37_10_t0"."S" = "s_37_10_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t2"
    ON (1)
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t3"
    ON (
      "s_37_10_t0"."S" = "s_37_10_t3"."O"
      AND 
      "s_37_10_t1"."S" = "s_37_10_t3"."O"
      AND 
      "s_37_10_t2"."S" = "s_37_10_t3"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t4"
    ON (
      "s_37_10_t2"."S" = "s_37_10_t4"."S"
      AND 
      "s_37_10_t3"."S" = "s_37_10_t4"."S")
  WHERE
    "s_37_10_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P21' , 1))
    AND 
    "s_37_10_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q6581072' , 1))
    AND 
    "s_37_10_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P166' , 1))
    AND 
    "s_37_10_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q47170' , 1))
    AND 
    "s_37_10_t2"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
    AND 
    "s_37_10_t2"."O" = __i2idn ( __bft( 'http://schema.org/Article' , 1))
    AND 
    "s_37_10_t3"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_10_t4"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
    AND 
    "s_37_10_t4"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'ca' )
OPTION (QUIETCAST)) AS "s_37_10_rbc"
