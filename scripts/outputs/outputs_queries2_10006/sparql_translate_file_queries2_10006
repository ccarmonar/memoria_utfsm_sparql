 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1", __ro2sq ("s_37_6_rbc"."var2") AS "var2" FROM (SELECT __id2in ( "s_37_6_t2"."P") AS "var1",
     COUNT (
       1) AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t1"."O" = "s_37_6_t2"."S")
  WHERE
    "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P296' , 1))
    AND 
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P296' , 1))
  GROUP BY "s_37_6_t2"."P"
OPTION (QUIETCAST)) AS "s_37_6_rbc"
