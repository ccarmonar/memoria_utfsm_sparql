SELECT TOP 1000 __id2in ( "s_37_4_t0"."S") AS "var1",
  __ro2sq ( "s_37_4_t0"."O") AS "var2",
  __ro2sq ( "s_37_4_t1"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."O" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P18' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/qualifier/P21' , 1))
OPTION (QUIETCAST)
