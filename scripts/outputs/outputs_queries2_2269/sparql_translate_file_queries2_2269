 SELECT  __ro2sq ("s_37_3_rbc"."var1Label") AS "var1Label" FROM (SELECT DISTINCT "s_37_2_t0"."S" AS "var1Label"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q27994917' , 1))
OPTION (QUIETCAST)) AS "s_37_3_rbc"
