 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://learningsparql.com/ns/demo#amount' ),
         1,
         1)),
      vector (
       "s_17_10"."s",
       "s_17_10"."newAmount"),
      vector (),
     1) AS "callret-0"
FROM (
   SELECT "s_17_9_t1"."S" AS "s",
      "s_17_9_t1"."O" AS "prodName",
      "s_17_9_t2"."O" AS "amount",
      "s_17_9_t3"."O" AS "units",
      DB.DBA.rdf_strdt_impl ( __ro2sq ( "s_17_9_t2"."O"),  __rdf_strsqlval ( rdf_concat_impl ( 'http://learningsparql.com/ns/units#' ,  __ro2sq ( "s_17_9_t3"."O")), 2)) AS "newAmount"
    FROM DB.DBA.RDF_QUAD AS "s_17_9_t1"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_17_9_t2"
      ON (
        "s_17_9_t1"."S" = "s_17_9_t2"."S")
      INNER JOIN DB.DBA.RDF_QUAD AS "s_17_9_t3"
      ON (
        "s_17_9_t1"."S" = "s_17_9_t3"."S"
        AND 
        "s_17_9_t2"."S" = "s_17_9_t3"."S")
    WHERE
      "s_17_9_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/importedData#product' , 1))
      AND 
      "s_17_9_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/importedData#amount' , 1))
      AND 
      "s_17_9_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/importedData#units' , 1))
OPTION (QUIETCAST)
    ) AS "s_17_10"
OPTION (QUIETCAST)))
