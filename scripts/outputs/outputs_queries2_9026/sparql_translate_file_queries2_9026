SELECT  COUNT (
     "s_37_5_t1"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t0"
  ON (
    "s_37_5_t1"."O" = "s_37_4_t0"."S")
WHERE
  "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
  AND 
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
  AND 
  "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q29583405' , 1))
OPTION (QUIETCAST)
