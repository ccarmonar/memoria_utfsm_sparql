SELECT __id2in ( "s_12_4_t0"."S") AS "s",
  __ro2sq ( "s_12_4_t1"."O") AS "cost",
  __ro2sq ( "s_12_4_t0"."O") AS "location"
FROM DB.DBA.RDF_QUAD AS "s_12_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_4_t1"
  ON (
    "s_12_4_t0"."S" = "s_12_4_t1"."S")
WHERE
  "s_12_4_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/demo#location' , 1))
  AND 
  "s_12_4_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/demo#cost' , 1))
  AND 
  not ( ( "s_12_4_t0"."O" in ( __i2idn ( __bft ( __bft( 'http://dbpedia.org/resource/Lisbon' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://dbpedia.org/resource/Montreal' , 1), 1)))))
OPTION (QUIETCAST)
