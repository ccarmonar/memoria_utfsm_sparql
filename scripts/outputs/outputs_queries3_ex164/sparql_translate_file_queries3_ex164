 SELECT  __ro2sq ("s_12_4_rbc"."description") AS "description", __ro2sq ("s_12_4_rbc"."mealTotal") AS "mealTotal" FROM (SELECT __ro2sq ( "s_12_4_t0"."O") AS "description",
     SUM (
       "s_12_4_t1"."O") AS "mealTotal"
  FROM DB.DBA.RDF_QUAD AS "s_12_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_12_4_t1"
    ON (
      "s_12_4_t0"."S" = "s_12_4_t1"."S")
  WHERE
    "s_12_4_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#description' , 1))
    AND 
    "s_12_4_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#amount' , 1))
  GROUP BY "s_12_4_t0"."O"
  HAVING( 20 <   SUM (
         "s_12_4_t1"."O"))
OPTION (QUIETCAST)) AS "s_12_4_rbc"
