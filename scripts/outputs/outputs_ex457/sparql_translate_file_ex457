 SELECT  __ro2sq ("s_7_2_rbc"."class") AS "class", __ro2sq ("s_7_2_rbc"."instanceTotal") AS "instanceTotal" FROM (SELECT __ro2sq ( "s_7_2_t0"."O") AS "class",
     COUNT (
       "s_7_2_t0"."S") AS "instanceTotal"
  FROM DB.DBA.RDF_QUAD AS "s_7_2_t0"
  WHERE
    "s_7_2_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
  GROUP BY "s_7_2_t0"."O"
  ORDER BY 2 DESC
OPTION (QUIETCAST)) AS "s_7_2_rbc"
