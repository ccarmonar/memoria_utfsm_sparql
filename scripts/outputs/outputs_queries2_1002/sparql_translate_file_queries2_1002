SELECT __id2in ( "s_37_3_t0"."S") AS "var1",
  __ro2sq ( "s_37_13_t2"."O") AS "var2",
  __ro2sq ( "s_37_13_t3"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t2"
  ON (
    "s_37_13_t2"."S" = "s_37_7_t1"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t3"
  ON (
    "s_37_13_t2"."S" = "s_37_13_t3"."S"
    AND 
    "s_37_13_t3"."S" = "s_37_7_t1"."O")
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P2102' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/value/P2102' , 1))
  AND 
  "s_37_13_t2"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#quantityAmount' , 1))
  AND 
  "s_37_13_t3"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#quantityUnit' , 1))
OPTION (QUIETCAST)
