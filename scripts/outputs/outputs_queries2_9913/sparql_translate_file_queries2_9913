 SELECT  "s_37_3_rbc"."var1" AS "var1", __ro2sq ("s_37_3_rbc"."var2") AS "var2" FROM (SELECT DISTINCT TOP 10 __bft( 'http://www.wikidata.org/entity/Q18042037' , 1) AS "var1",
    "s_37_3_t0"."S" AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  WHERE
    "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P702' , 1))
    AND 
    "s_37_3_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q18042037' , 1))
OPTION (QUIETCAST)) AS "s_37_3_rbc"
