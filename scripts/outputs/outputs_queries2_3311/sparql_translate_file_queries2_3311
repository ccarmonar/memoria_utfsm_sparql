 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1" FROM (SELECT DISTINCT __ro2sq ( "s_37_4_t1"."O") AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."O" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P220' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P220' , 1))
  ORDER BY __ro2sq ( "s_37_4_t1"."O") ASC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
