SELECT __ro2sq ( "s_37_12_t1"."O") AS "var1"
FROM (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_4_t0"."S" AS "_::trans_subj_37_0",
      "s_37_4_t0"."O" AS "_::trans_obj_37_1"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_7"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t1"
  ON (
    "s_37_12_t1"."O" = "s_37_7"."_::trans_obj_37_1")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t2"
  ON (
    "s_37_12_t1"."S" = "s_37_12_t2"."O")
WHERE
  "s_37_12_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_12_t2"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
 ( "s_37_7"."_::trans_subj_37_0" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q198' , 1)))
  AND 
  (cast ( __bft (__id2in ( "s_37_12_t2"."S"), 2) as varchar) like  '%string1%' )
  AND 
  1
OPTION (QUIETCAST)
