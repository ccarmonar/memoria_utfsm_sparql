SELECT __id2in ( "s_37_7_t1"."P") AS "var1",
  __ro2sq ( "s_37_7_t1"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."S")
WHERE
  "s_37_3_t0"."O" = DB.DBA.RDF_MAKE_OBJ_OF_TYPEDSQLVAL ( 'string1' , __i2id( NULL),  'fr' )
  AND 
  ( "s_37_3_t0"."P" in ( __i2idn ( __bft ( __bft( 'http://www.w3.org/2000/01/rdf-schema#label' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.w3.org/2004/02/skos/core#altLabel' , 1), 1))))
OPTION (QUIETCAST)
