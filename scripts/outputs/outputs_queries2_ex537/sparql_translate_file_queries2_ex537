SELECT __id2in ( "s_11_4_t0"."S") AS "property",
  __ro2sq ( "s_11_4_t1"."O") AS "propName"
FROM DB.DBA.RDF_QUAD AS "s_11_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_11_4_t1"
  ON (
    "s_11_4_t0"."S" = "s_11_4_t1"."S")
WHERE
  "s_11_4_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2000/01/rdf-schema#domain' , 1))
  AND 
  "s_11_4_t0"."O" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#Person' , 1))
  AND 
  "s_11_4_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2000/01/rdf-schema#label' , 1))
OPTION (QUIETCAST)
