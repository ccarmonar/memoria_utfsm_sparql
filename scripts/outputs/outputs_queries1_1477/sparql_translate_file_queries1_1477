SELECT __ro2sq ( "s_37_8_t4"."O") AS "var1",
  NULL /* due to conflict on var2Label */ AS "var2Label"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
  ON (
    "s_37_8_t4"."O" = "s_37_8_t3"."S")
  LEFT OUTER JOIN (
    SELECT "s_37_6_t0"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    WHERE
      "s_37_6_t0"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
      AND 
     ( 'en,ro'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_6_t0"."O")))) AS "s_37_6"
  ON (
    "s_37_8_t4"."O" = "s_37_6"."var1"
    AND 
    "s_37_8_t3"."S" = "s_37_6"."var1")
WHERE
  "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P3982' , 1))
  AND 
  "s_37_8_t4"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
OPTION (QUIETCAST)
