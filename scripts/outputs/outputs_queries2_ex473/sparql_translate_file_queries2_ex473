SELECT __id2in ( "s_8_2_t0"."S") AS "s",
  __id2in ( "s_8_2_t0"."P") AS "p",
  __ro2sq ( "s_8_2_t0"."O") AS "o"
FROM DB.DBA.RDF_QUAD AS "s_8_2_t0"
WHERE
  rdf_contains_impl ( rdf_lcase_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( __rdf_strsqlval ( "s_8_2_t0"."O", 0))),  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'publish' ))
OPTION (QUIETCAST)
