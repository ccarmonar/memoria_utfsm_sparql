SELECT NULL /* due to conflict on var1 */ AS "var1",
  __id2in ( "s_37_3_t0"."S") AS "var2",
  NULL /* due to conflict on var2Label */ AS "var2Label"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  LEFT OUTER JOIN (
    SELECT NULL AS __dummy_retval
    FROM
      (SELECT TOP 1 1 AS __stub FROM DB.DBA.SYS_IDONLY_EMPTY WHERE 0) AS "stub-s_37_2") AS "s_37_2"
  ON (1)
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_3_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q22686' , 1))
OPTION (QUIETCAST)
