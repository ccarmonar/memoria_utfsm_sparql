SELECT __id2in ( "s_37_3_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
  ON (
    "s_37_8_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_14_t2"."S" AS "_::trans_subj_37_10",
      "s_37_14_t2"."O" AS "_::trans_obj_37_11"
    FROM DB.DBA.RDF_QUAD AS "s_37_14_t2"
    WHERE
      "s_37_14_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_17"
  ON (
    "s_37_8_t1"."O" = "s_37_17"."_::trans_subj_37_10")
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_8_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P31' , 1))
  AND 
  isiri_id ( "s_37_8_t1"."O")
  AND 
 ( "s_37_17"."_::trans_obj_37_11" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12280' , 1)))
  AND 
  1
OPTION (QUIETCAST)
