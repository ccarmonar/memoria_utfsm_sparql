SELECT TOP 50 __id2in ( "s_37_8_t3"."S") AS "var1",
  __id2in ( "s_37_8_t3"."P") AS "var2",
  __ro2sq ( "s_37_8_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
  ON (
    "s_37_8_t0"."S" = "s_37_8_t1"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t2"
  ON (
    "s_37_8_t0"."O" = "s_37_8_t2"."O"
    AND 
    "s_37_8_t1"."S" = "s_37_8_t2"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t3"
  ON (
    "s_37_8_t1"."S" = "s_37_8_t3"."O"
    AND 
    "s_37_8_t2"."S" = "s_37_8_t3"."O")
WHERE
  "s_37_8_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P143' , 1))
  AND 
  "s_37_8_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
OPTION (QUIETCAST)
