SELECT __ro2sq ( "s_37_2_t0"."O") AS "var1",
  NULL /* due to conflict on var2 */ AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q22686' , 1))
  AND 
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
OPTION (QUIETCAST)
