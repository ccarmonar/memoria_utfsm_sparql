SELECT __ro2sq ( "s_37_4_t0"."O") AS "var1",
  NULL /* due to conflict on var2 */ AS "var2",
  __ro2sq ( "s_37_4_t1"."O") AS "var3Label"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P297' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P1705' , 1))
OPTION (QUIETCAST)
