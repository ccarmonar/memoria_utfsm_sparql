SELECT __ro2sq ( "s_11_4_t0"."O") AS "filename",
  __ro2sq ( "s_11_4_t1"."O") AS "creator"
FROM DB.DBA.RDF_QUAD AS "s_11_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_11_4_t1"
  ON (
    "s_11_4_t0"."S" = "s_11_4_t1"."S")
WHERE
  "s_11_4_t0"."P" = __i2idn ( __bft( 'http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#fileName' , 1))
  AND 
  "s_11_4_t1"."P" = __i2idn ( __bft( 'http://purl.org/dc/elements/1.1/creator' , 1))
OPTION (QUIETCAST)
