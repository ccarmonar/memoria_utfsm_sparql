SELECT __ro2sq ( "s_37_4_t1"."O") AS "var2Label"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (1)
WHERE
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q3052772' , 1))
  AND 
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q5' , 1))
  AND 
  "s_37_4_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q3052772' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
  AND 
 ( 'en'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_4_t1"."O")))
OPTION (QUIETCAST)
