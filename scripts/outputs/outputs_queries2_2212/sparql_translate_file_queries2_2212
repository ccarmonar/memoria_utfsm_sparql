SELECT __ro2sq ( "s_37_2_t0"."O") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/P1083' , 1))
  AND 
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2004/02/skos/core#altLabel' , 1))
  AND 
 ( 'fr'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O")))
OPTION (QUIETCAST)
