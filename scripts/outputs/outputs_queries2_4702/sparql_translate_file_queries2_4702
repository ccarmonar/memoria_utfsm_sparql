SELECT __bft( 'http://www.wikidata.org/entity/Q3083241' , 1) AS "var1",
  __id2in ( "s_37_3_t0"."P") AS "var2",
  __ro2sq ( "s_37_3_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q3083241' , 1))
  AND 
  not ( DB.DBA.RDF_LANGMATCHES ( DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_3_t0"."O")), '*' ))
OPTION (QUIETCAST)
