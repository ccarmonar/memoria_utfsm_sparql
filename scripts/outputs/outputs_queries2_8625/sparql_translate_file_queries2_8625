 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1" FROM (SELECT __id2in ( "s_37_2_t0"."S") AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P238' , 1))
    AND 
    "s_37_2_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'BRU' )
  ORDER BY __id2in ( "s_37_2_t0"."S") ASC
OPTION (QUIETCAST)) AS "s_37_2_rbc"
