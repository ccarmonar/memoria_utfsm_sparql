SELECT __ro2sq ( "s_37_4_t0"."O") AS "var1",
  __ro2sq ( "s_37_4_t1"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (1)
WHERE
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://dbpedia.org/resource/Bologna' , 1))
  AND 
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2003/01/geo/wgs84_pos#lat' , 1))
  AND 
  "s_37_4_t1"."S" = __i2idn ( __bft( 'http://dbpedia.org/resource/Bologna' , 1))
  AND 
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2003/01/geo/wgs84_pos#long' , 1))
OPTION (QUIETCAST)
