SELECT  COUNT (
     "s_0_12"."var1") AS "var2"
FROM (
    SELECT "s_37_9_t2-u13"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_9_t2-u13"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t0-c5-u13"
      ON (
        "s_37_9_t2-u13"."O" = "s_37_4_t0-c5-u13"."S")
    WHERE
      "s_37_9_t2-u13"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
      AND 
      "s_37_4_t0-c5-u13"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
      AND 
      "s_37_4_t0-c5-u13"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q29580583' , 1))
    UNION ALL SELECT "s_37_9_t2"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_9_t2"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1-c11"
      ON (
        "s_37_9_t2"."O" = "s_37_7_t1-c11"."S")
    WHERE
      "s_37_9_t2"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
      AND 
      "s_37_7_t1-c11"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
      AND 
      "s_37_7_t1-c11"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q29583405' , 1))) AS "s_0_12"
OPTION (QUIETCAST)
