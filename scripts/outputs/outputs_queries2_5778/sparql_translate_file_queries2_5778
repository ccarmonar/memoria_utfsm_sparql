SELECT __id2in ( "s_0_12"."var1") AS "var1",
  __ro2sq ( "s_0_12"."var2") AS "var2"
FROM (
    SELECT "s_37_9_t2-u13"."S" AS "var1",
      "s_37_9_t2-u13"."O" AS "var2" ANY
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0-c5-u13"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_9_t2-u13"
      ON (
        "s_37_9_t2-u13"."S" = "s_37_2_t0-c5-u13"."S")
    WHERE
      "s_37_2_t0-c5-u13"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_2_t0-c5-u13"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1426271' , 1))
      AND 
      "s_37_9_t2-u13"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))
    UNION ALL SELECT "s_37_9_t2"."S" AS "var1",
      "s_37_9_t2"."O" AS "var2"
    FROM DB.DBA.RDF_QUAD AS "s_37_5_t1-c11"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_9_t2"
      ON (
        "s_37_9_t2"."S" = "s_37_5_t1-c11"."S")
    WHERE
      "s_37_5_t1-c11"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P16' , 1))
      AND 
      "s_37_5_t1-c11"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1426271' , 1))
      AND 
      "s_37_9_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))) AS "s_0_12"
OPTION (QUIETCAST)
