 SELECT  __ro2sq ("s_37_4_rbc"."p1") AS "p1" FROM (SELECT DISTINCT "s_37_4_t1"."P" AS "p1"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."O" = "s_37_4_t1"."O")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P3726' , 1))
    AND 
   ( "s_37_4_t1"."P" <>  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P3726' , 1), 1)))
OPTION (QUIETCAST)) AS "s_37_4_rbc"
