SELECT __id2in ( "s_37_15_t3"."S") AS "var1",
  __ro2sq ( "s_37_15_t3"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_15_t3"
  ON (
    "s_37_15_t3"."S" = "s_37_3_t0"."S")
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
  AND 
  "s_37_7_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4830453' , 1))
  AND 
  "s_37_15_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2427' , 1))
  AND 
  not ( EXISTS ( (
     SELECT TOP 1 1 AS __ask_retval
      FROM DB.DBA.RDF_QUAD AS "s_37_13_t2"
      WHERE
        "s_37_13_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P159' , 1))
        AND 
        "s_37_13_t2"."S" = "s_37_3_t0"."S"
OPTION (QUIETCAST)
     )))
OPTION (QUIETCAST)
