SELECT TOP 1 1 AS __ask_retval
FROM DB.DBA.RDF_QUAD AS "s_10_2_t0"
WHERE
  "s_10_2_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/demo#amount' , 1))
  AND 
 ( __bft ( __bft( 'http://www.w3.org/2001/XMLSchema#integer' , 1), 1) <>  DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_10_2_t0"."O")))
OPTION (QUIETCAST)
