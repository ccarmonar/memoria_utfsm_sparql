SELECT __id2in ( "s_37_6"."var1") AS "var1",
  NULL /* due to conflict on var2Label */ AS "var2Label",
  NULL /* due to conflict on var3 */ AS "var3"
FROM (
    SELECT "s_37_2_t0-u8"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0-u8"
    WHERE
      "s_37_2_t0-u8"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_2_t0-u8"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q571' , 1))
    UNION ALL SELECT "s_37_5_t1"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
    WHERE
      "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_5_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q7725634' , 1))) AS "s_37_6"
OPTION (QUIETCAST)
