 SELECT  __ro2sq ("s_37_7_rbc"."var1") AS "var1" FROM (SELECT DISTINCT TOP 1 "s_37_6"."var1" AS "var1"
  FROM (
      SELECT "s_37_2_t0-u8"."S" AS "var1" ANY
      FROM DB.DBA.RDF_QUAD AS "s_37_2_t0-u8"
      WHERE
        "s_37_2_t0-u8"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
        AND 
        "s_37_2_t0-u8"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q864380' , 1))
      UNION ALL SELECT "s_37_5_t1"."O" AS "var1"
      FROM DB.DBA.RDF_QUAD AS "s_37_5_t1"
      WHERE
        "s_37_5_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q864380' , 1))
        AND 
        "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P373' , 1))) AS "s_37_6"
OPTION (QUIETCAST)) AS "s_37_7_rbc"
