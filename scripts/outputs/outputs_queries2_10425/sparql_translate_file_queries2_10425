 SELECT  __ro2sq ("s_37_5_rbc"."var1") AS "var1", __ro2sq ("s_37_5_rbc"."var2") AS "var2", __ro2sq ("s_37_5_rbc"."var3") AS "var3" FROM (SELECT DISTINCT "s_37_5_t1"."P" AS "var1",
    "s_37_5_t0"."O" AS "var2",
    "s_37_5_t1"."O" AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_5_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t1"
    ON (
      "s_37_5_t0"."O" = "s_37_5_t1"."S")
  WHERE
    "s_37_5_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q725534' , 1))
    AND 
    DB.DBA.RDF_LANGMATCHES ( DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_5_t1"."O")), 'fr' )
OPTION (QUIETCAST)) AS "s_37_5_rbc"
