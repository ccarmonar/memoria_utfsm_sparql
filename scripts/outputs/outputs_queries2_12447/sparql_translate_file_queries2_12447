SELECT __id2in ( "s_37_12_t1"."S") AS "var1",
  __ro2sq ( "s_37_12_t3"."O") AS "var2",
  __id2in ( "s_37_12_t4"."S") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_12_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t2"
  ON (
    "s_37_12_t1"."S" = "s_37_12_t2"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t3"
  ON (
    "s_37_12_t1"."S" = "s_37_12_t3"."S"
    AND 
    "s_37_12_t2"."S" = "s_37_12_t3"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t0"
  ON (
    "s_37_12_t3"."O" = "s_37_8_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t4"
  ON (
    "s_37_12_t4"."S" = "s_37_8_t0"."S")
WHERE
  "s_37_12_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_12_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q55488' , 1))
  AND 
  "s_37_12_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P17' , 1))
  AND 
  "s_37_12_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q213' , 1))
  AND 
  "s_37_12_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
  AND 
  "s_37_8_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
  AND 
  "s_37_12_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_12_t4"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q55488' , 1))
OPTION (QUIETCAST)
