 SELECT  __ro2sq ("s_37_30_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_3_t0"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
    INNER JOIN (
     SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_9_t1"."S" AS "_::trans_subj_37_5",
        "s_37_9_t1"."O" AS "_::trans_obj_37_6"
      FROM DB.DBA.RDF_QUAD AS "s_37_9_t1"
      WHERE
        "s_37_9_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
      ) AS "s_37_12"
    ON (
      "s_37_3_t0"."O" = "s_37_12"."_::trans_subj_37_5")
    INNER JOIN (
     SELECT "s_37_27"."var1" AS "var1"
      FROM (
          SELECT "s_37_15_t2"."S" AS "var1" ANY
          FROM DB.DBA.RDF_QUAD AS "s_37_15_t2"
          WHERE
            "s_37_15_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
            AND 
            "s_37_15_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1198' , 1))
          UNION ALL SELECT "s_37_23"."_::trans_obj_37_17"
          FROM (
             SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_20_t3"."S" AS "_::trans_subj_37_16",
                "s_37_20_t3"."O" AS "_::trans_obj_37_17"
              FROM DB.DBA.RDF_QUAD AS "s_37_20_t3"
              WHERE
                "s_37_20_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P150' , 1))
OPTION (QUIETCAST)
              ) AS "s_37_23"
            INNER JOIN DB.DBA.RDF_QUAD AS "s_37_26_t4"
            ON (
              "s_37_26_t4"."S" = "s_37_23"."_::trans_subj_37_16")
          WHERE
            "s_37_26_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
            AND 
            "s_37_26_t4"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1198' , 1))
            AND 
            1
            AND 
            1) AS "s_37_27"
OPTION (QUIETCAST)
      ) AS "s_37_29"
    ON (
      "s_37_3_t0"."S" = "s_37_29"."var1")
  WHERE
    "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    isiri_id ( "s_37_3_t0"."O")
    AND 
   ( "s_37_12"."_::trans_obj_37_6" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q387917' , 1)))
    AND 
    1
OPTION (QUIETCAST)) AS "s_37_30_rbc"
