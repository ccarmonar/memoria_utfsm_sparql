SELECT __id2in ( "s_37_15_t2"."S") AS "var1",
  NULL /* due to conflict on var1Label */ AS "var1Label"
FROM DB.DBA.RDF_QUAD AS "s_37_15_t2"
  INNER JOIN (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_7_t0"."S" AS "_::trans_subj_37_3",
      "s_37_7_t0"."O" AS "_::trans_obj_37_4"
    FROM DB.DBA.RDF_QUAD AS "s_37_7_t0"
    WHERE
      "s_37_7_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_10"
  ON (
    "s_37_15_t2"."S" = "s_37_10"."_::trans_subj_37_3")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t1"
  ON (
    "s_37_13_t1"."O" = "s_37_10"."_::trans_obj_37_4")
WHERE
  "s_37_15_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_15_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q2074737' , 1))
  AND 
  "s_37_13_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q47588' , 1))
  AND 
  "s_37_13_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P527' , 1))
  AND 
  1
  AND 
  1
OPTION (QUIETCAST)
