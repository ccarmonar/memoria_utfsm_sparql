 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://xmlns.com/foaf/0.1/givenName' ),
         1,
         3),
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://xmlns.com/foaf/0.1/familyName' ),
         1,
         2),
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://xmlns.com/foaf/0.1/mbox_sha1sum' ),
         1,
         1)),
      vector (
       "s_17_14"."s",
       DB.DBA.RDF_LONG_OF_SQLVAL ( "s_17_14"."hashEmail"),
       __ro2lo ( "s_17_14"."last"),
       __ro2lo ( "s_17_14"."first")),
      vector (),
     1) AS "callret-0"
FROM (
   SELECT "s_17_13_t3"."S" AS "s",
      "s_17_13_t3"."O" AS "first",
      "s_17_13_t4"."O" AS "last",
      "s_17_13_t5"."O" AS "email",
      rdf_sha1_impl ( __ro2sq ( "s_17_13_t5"."O")) AS "hashEmail"
    FROM DB.DBA.RDF_QUAD AS "s_17_13_t3"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_17_13_t4"
      ON (
        "s_17_13_t3"."S" = "s_17_13_t4"."S")
      INNER JOIN DB.DBA.RDF_QUAD AS "s_17_13_t5"
      ON (
        "s_17_13_t3"."S" = "s_17_13_t5"."S"
        AND 
        "s_17_13_t4"."S" = "s_17_13_t5"."S")
    WHERE
      "s_17_13_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
      AND 
      "s_17_13_t4"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
      AND 
      "s_17_13_t5"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
OPTION (QUIETCAST)
    ) AS "s_17_14"
OPTION (QUIETCAST)))
