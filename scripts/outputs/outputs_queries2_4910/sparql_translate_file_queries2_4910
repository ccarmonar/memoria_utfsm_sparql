SELECT __bft( 'http://www.wikidata.org/entity/statement/Q586211-275747CB-D5AF-4021-A3C3-9693C9EF62B5' , 1) AS "var1",
  __id2in ( "s_37_3_t0"."P") AS "var2",
  __ro2sq ( "s_37_3_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/statement/Q586211-275747CB-D5AF-4021-A3C3-9693C9EF62B5' , 1))
OPTION (QUIETCAST)
