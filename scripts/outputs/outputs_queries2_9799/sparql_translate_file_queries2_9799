SELECT __id2in ( "s_37_6_t1"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t1"
WHERE
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P972' , 1))
  AND 
  "s_37_6_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q16986324' , 1))
  AND 
  EXISTS ( (
     SELECT TOP 1 1 AS __ask_retval
      FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
      WHERE
        "s_37_4_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
        AND 
        "s_37_4_t0"."O" = "s_37_6_t1"."S"
OPTION (QUIETCAST)
     ))
OPTION (QUIETCAST)
