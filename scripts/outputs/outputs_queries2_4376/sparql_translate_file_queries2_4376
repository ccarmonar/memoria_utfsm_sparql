SELECT __id2in ( "s_37_11_t2"."P") AS "var1",
  __ro2sq ( "s_37_11_t2"."O") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (
    "s_37_7_t1"."S" = "s_37_3_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_11_t2"
  ON (
    "s_37_11_t2"."S" = "s_37_7_t1"."O")
WHERE
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
  AND 
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P18' , 1))
  AND 
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P18' , 1))
OPTION (QUIETCAST)
