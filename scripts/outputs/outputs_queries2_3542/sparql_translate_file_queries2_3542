SELECT __id2in ( "s_37_2_t0"."P") AS "var1",
  __id2in ( "s_37_2_t0"."S") AS "var2",
  case when ( 1) then ( 'IRI' ) else ( case when ( is_bnode_iri_id ( "s_37_2_t0"."S")) then ( 'blank node' ) else ( 'string2' ) end) end AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12418' , 1))
OPTION (QUIETCAST)
