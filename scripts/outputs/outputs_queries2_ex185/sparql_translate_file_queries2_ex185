 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         1,
         2,
         1,
         3),
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://learningsparql.com/ns/addressbook#areaCode' ),
         1,
         1)),
      vector (
       "s_15_10"."person",
       DB.DBA.RDF_LONG_OF_SQLVAL ( "s_15_10"."areaCode"),
       "s_15_10"."p",
       __ro2lo ( "s_15_10"."o")),
      vector (),
     1) AS "callret-0"
FROM (
   SELECT "s_15_9_t2"."S" AS "person",
      "s_15_9_t2"."O" AS "phone",
      "s_15_9_t3"."P" AS "p",
      "s_15_9_t3"."O" AS "o",
      rdf_substr_impl ( __ro2lo ( "s_15_9_t2"."O"),  2,  3) AS "areaCode"
    FROM DB.DBA.RDF_QUAD AS "s_15_9_t2"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_15_9_t3"
      ON (
        "s_15_9_t2"."S" = "s_15_9_t3"."S")
    WHERE
      "s_15_9_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#homeTel' , 1))
OPTION (QUIETCAST)
    ) AS "s_15_10"
OPTION (QUIETCAST)))
