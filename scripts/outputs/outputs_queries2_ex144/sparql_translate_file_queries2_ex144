SELECT __rdf_sqlval_of_obj /*l*/ ( "s_11_5"."mealCode") AS "mealCode",
  __ro2sq ( "s_11_5"."amount") AS "amount"
FROM (
   SELECT "s_11_4_t0"."S" AS "meal",
      "s_11_4_t0"."O" AS "description",
      "s_11_4_t1"."O" AS "amount",
      rdf_ucase_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( rdf_substr_impl ( __ro2lo ( "s_11_4_t0"."O"),  1,  3))) AS "mealCode"
    FROM DB.DBA.RDF_QUAD AS "s_11_4_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_11_4_t1"
      ON (
        "s_11_4_t0"."S" = "s_11_4_t1"."S")
    WHERE
      "s_11_4_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#description' , 1))
      AND 
      "s_11_4_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/expenses#amount' , 1))
OPTION (QUIETCAST)
    ) AS "s_11_5"
OPTION (QUIETCAST)
