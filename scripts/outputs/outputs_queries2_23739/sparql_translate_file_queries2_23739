 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var2") AS "var2", __ro2sq ("s_37_4_rbc"."var3") AS "var3", __ro2sq ("s_37_4_rbc"."var4") AS "var4" FROM (SELECT TOP 1 __id2in ( "s_37_4_t0"."S") AS "var1",
    __ro2sq ( "s_37_4_t0"."O") AS "var2",
    NULL /* due to conflict on var3 */ AS "var3",
    NULL /* due to conflict on var4 */ AS "var4"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P18' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P625' , 1))
  ORDER BY rdf_rand_impl () ASC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
