SELECT NULL /* due to conflict on var1 */ AS "var1",
  NULL /* due to conflict on var2 */ AS "var2",
  NULL /* due to conflict on var3 */ AS "var3",
  NULL /* due to conflict on var4 */ AS "var4",
  NULL /* due to conflict on var5 */ AS "var5",
  NULL /* due to conflict on var6 */ AS "var6",
  NULL /* due to conflict on var7 */ AS "var7",
  NULL /* due to conflict on var8 */ AS "var8",
  NULL /* due to conflict on var9 */ AS "var9"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t2"."S"
    AND 
    "s_37_6_t1"."S" = "s_37_6_t2"."S")
WHERE
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
  AND 
  "s_37_6_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q7187' , 1))
  AND 
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P703' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P351' , 1))
  AND 
  "s_37_6_t2"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( '882855' )
OPTION (QUIETCAST)
