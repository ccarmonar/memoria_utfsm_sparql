SELECT __id2in ( "s_37_4_t0"."S") AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2Label",
  __id2in ( "s_37_4_t1"."P") AS "var3",
  __ro2sq ( "s_37_4_t1"."O") AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t0"."O" = DB.DBA.RDF_MAKE_OBJ_OF_TYPEDSQLVAL ( 'Tom Hanks' , __i2id( NULL),  'en' )
OPTION (QUIETCAST)
