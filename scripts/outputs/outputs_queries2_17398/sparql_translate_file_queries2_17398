SELECT __ro2sq ( "s_37_3"."var2") AS "var2",
  __id2in ( "s_37_3"."var4") AS "var4"
FROM (
   SELECT "s_37_2_t0"."O" AS "var2",
      "s_37_2_t0"."S" AS "var4"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
    WHERE
      "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P815' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_3"
OPTION (QUIETCAST)
