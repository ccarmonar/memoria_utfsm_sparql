SELECT __id2in ( "s_37_8_t1"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t2"
  ON (
    "s_37_8_t1"."S" = "s_37_8_t2"."S")
WHERE
  "s_37_8_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P27' , 1))
  AND 
  "s_37_8_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q39' , 1))
  AND 
  "s_37_8_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_8_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q5' , 1))
  AND 
  not ( EXISTS ( (
     SELECT TOP 1 1 AS __ask_retval
      FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
      WHERE
        "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P734' , 1))
        AND 
        "s_37_6_t0"."S" = "s_37_8_t2"."S"
OPTION (QUIETCAST)
     )))
OPTION (QUIETCAST)
