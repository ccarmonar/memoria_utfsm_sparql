 SELECT  __ro2sq ("s_37_9_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_8"."var1" AS "var1"
  FROM (
      SELECT "s_37_4_t1"."S" AS "var1"
      FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
        INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
        ON (
          "s_37_4_t0"."S" = "s_37_4_t1"."O")
      WHERE
        "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
        AND 
        "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12280' , 1))
        AND 
        "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      UNION ALL SELECT "s_37_7_t2"."S" AS "var1"
      FROM DB.DBA.RDF_QUAD AS "s_37_7_t2"
      WHERE
        "s_37_7_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
        AND 
        "s_37_7_t2"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12280' , 1))) AS "s_37_8"
OPTION (QUIETCAST)) AS "s_37_9_rbc"
