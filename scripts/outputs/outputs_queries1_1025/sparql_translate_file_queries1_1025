SELECT __id2in ( "s_37_16"."var1") AS "var1",
  __ro2sq ( "s_37_16"."var2") AS "var2",
  __ro2sq ( "s_37_16"."var3") AS "var3",
  COALESCE (
     __ro2sq ( "s_37_16"."var4"),
     __ro2sq ( "s_37_12"."var4")) AS "var4"
FROM (
    SELECT "s_37_14_t6"."S" AS "var1",
      "s_37_14_t6"."O" AS "var2",
      "s_37_6"."var3" AS "var3",
      "s_37_9"."var4" AS "var4"
    FROM DB.DBA.RDF_QUAD AS "s_37_14_t5"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_14_t6"
      ON (
        "s_37_14_t6"."S" = "s_37_14_t5"."S")
      LEFT OUTER JOIN (
        SELECT "s_37_6_t0"."S" AS "var1",
          "s_37_6_t0"."O" AS "var3"
        FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
        WHERE
          "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P577' , 1))) AS "s_37_6"
      ON (
        "s_37_14_t6"."S" = "s_37_6"."var1"
        AND 
        "s_37_14_t5"."S" = "s_37_6"."var1")
      LEFT OUTER JOIN (
        SELECT "s_37_9_t1"."S" AS "var1",
          "s_37_9_t1"."O" AS "var4"
        FROM DB.DBA.RDF_QUAD AS "s_37_9_t1"
        WHERE
          "s_37_9_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P179' , 1))) AS "s_37_9"
      ON (
        "s_37_14_t6"."S" = "s_37_9"."var1"
        AND 
        "s_37_14_t5"."S" = "s_37_9"."var1")
    WHERE
      "s_37_14_t5"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P50' , 1))
      AND 
      "s_37_14_t5"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q520852' , 1))
      AND 
      "s_37_14_t6"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))) AS "s_37_16"
  LEFT OUTER JOIN (
    SELECT "s_37_12_t2"."S" AS "var1",
      "s_37_12_t2"."O" AS "var4"
    FROM DB.DBA.RDF_QUAD AS "s_37_12_t2"
    WHERE
      "s_37_12_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P361' , 1))) AS "s_37_12"
  ON (
    "s_37_16"."var1" = "s_37_12"."var1"
    AND 
    ( "s_37_16"."var4" IS NULL  OR ( "s_37_16"."var4" = "s_37_12"."var4" ) ))
OPTION (QUIETCAST)
