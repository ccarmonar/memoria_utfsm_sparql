SELECT __ro2sq ( "s_15_5"."lname") AS "lname",
  __ro2sq ( "s_15_5"."courseName") AS "courseName"
FROM (
    SELECT "s_13_1_t0-u7"."O" AS "lname" ANY,
      NULL AS "courseName" ANY
    FROM DB.DBA.RDF_QUAD AS "s_13_1_t0-u7"
    WHERE
      "s_13_1_t0-u7"."G" = __i2idn ( __bft( 'ex069.ttl' , 1))
      AND 
      "s_13_1_t0-u7"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
    UNION ALL SELECT NULL AS "lname",
      "s_14_3_t1"."O" AS "courseName"
    FROM DB.DBA.RDF_QUAD AS "s_14_3_t1"
    WHERE
      "s_14_3_t1"."G" = __i2idn ( __bft( 'ex125.ttl' , 1))
      AND 
      "s_14_3_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#courseTitle' , 1))
      AND 
      1) AS "s_15_5"
OPTION (QUIETCAST)
