SELECT TOP 1 __id2in ( "s_37_2_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P569' , 1))
  AND 
 ( rdf_month_impl ( "s_37_2_t0"."O") =  rdf_month_impl ( rdf_now_impl ()))
  AND 
 ( rdf_day_impl ( "s_37_2_t0"."O") =  rdf_day_impl ( rdf_now_impl ()))
  AND 
 (( __bft ( __bft( 'http://www.w3.org/2001/XMLSchema#date' , 1), 1) =  DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O"))) OR 
   ( __bft ( __bft( 'http://www.w3.org/2001/XMLSchema#dateTime' , 1), 1) =  DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O"))))
OPTION (QUIETCAST)
