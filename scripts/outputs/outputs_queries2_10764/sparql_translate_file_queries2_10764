SELECT NULL /* due to conflict on var1 */ AS "var1",
  NULL /* due to conflict on var1Label */ AS "var1Label",
  NULL /* due to conflict on var2 */ AS "var2",
  NULL /* due to conflict on var3Label */ AS "var3Label"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q9188' , 1))
  AND 
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P625' , 1))
OPTION (QUIETCAST)
