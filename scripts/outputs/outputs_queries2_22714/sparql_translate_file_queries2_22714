 SELECT  __ro2sq ("s_37_13_rbc"."var1") AS "var1" FROM (SELECT DISTINCT TOP 500 "s_37_13_t1"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_13_t1"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t2"
    ON (
      "s_37_13_t1"."S" = "s_37_13_t2"."S")
    INNER JOIN (
     SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_9_t0"."S" AS "_::trans_subj_37_4",
        "s_37_9_t0"."O" AS "_::trans_obj_37_5"
      FROM DB.DBA.RDF_QUAD AS "s_37_9_t0"
      WHERE
        ( "s_37_9_t0"."P" in ( __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1), 1))))
OPTION (QUIETCAST)
      ) AS "s_37_12"
    ON (
      "s_37_13_t2"."O" = "s_37_12"."_::trans_subj_37_4")
  WHERE
    "s_37_13_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_13_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q5' , 1))
    AND 
    "s_37_13_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P463' , 1))
    AND 
    isiri_id ( "s_37_13_t2"."O")
    AND 
   ( "s_37_12"."_::trans_obj_37_5" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q253439' , 1)))
    AND 
    1
OPTION (QUIETCAST)) AS "s_37_13_rbc"
