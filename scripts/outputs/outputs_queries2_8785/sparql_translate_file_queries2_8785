 SELECT  __ro2sq ("s_37_7_rbc"."var1") AS "var1", __ro2sq ("s_37_7_rbc"."var2") AS "var2" FROM (SELECT __ro2sq ( "s_37_8_t4"."O") AS "var1",
    __ro2sq ( "s_37_6"."var2") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_8_t3"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t4"
    ON (
      "s_37_8_t4"."S" = "s_37_8_t3"."O")
    LEFT OUTER JOIN (
      SELECT "s_37_6_t0"."S" AS "var3",
        "s_37_6_t0"."O" AS "var2"
      FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
      WHERE
        "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P220' , 1))) AS "s_37_6"
    ON (
      "s_37_8_t3"."S" = "s_37_6"."var3")
  WHERE
    "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P220' , 1))
    AND 
    "s_37_8_t4"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P220' , 1))
  ORDER BY __ro2sq ( "s_37_6"."var2") ASC, __ro2sq ( "s_37_8_t4"."O") ASC
OPTION (QUIETCAST)) AS "s_37_7_rbc"
