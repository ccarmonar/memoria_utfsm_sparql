 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var2") AS "var2" FROM (SELECT __ro2sq ( "s_37_4_t1"."O") AS "var1",
    __ro2sq ( "s_37_4_t0"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P1628' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
    AND 
    rdf_contains_impl ( DB.DBA.RDF_LONG_OF_SQLVAL ( __rdf_strsqlval ( "s_37_4_t0"."O", 0)),  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'string1' ))
  ORDER BY __ro2sq ( "s_37_4_t0"."O") ASC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
