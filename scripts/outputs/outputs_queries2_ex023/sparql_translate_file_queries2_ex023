SELECT __ro2sq ( "s_12_8_t2"."O") AS "craigEmail",
  __ro2sq ( "s_12_8_t3"."O") AS "homeTel"
FROM DB.DBA.RDF_QUAD AS "s_12_8_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_8_t1"
  ON (
    "s_12_8_t0"."S" = "s_12_8_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_8_t2"
  ON (
    "s_12_8_t0"."S" = "s_12_8_t2"."S"
    AND 
    "s_12_8_t1"."S" = "s_12_8_t2"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_8_t3"
  ON (
    "s_12_8_t0"."S" = "s_12_8_t3"."S"
    AND 
    "s_12_8_t1"."S" = "s_12_8_t3"."S"
    AND 
    "s_12_8_t2"."S" = "s_12_8_t3"."S")
WHERE
  "s_12_8_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
  AND 
  "s_12_8_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'Craig' )
  AND 
  "s_12_8_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
  AND 
  "s_12_8_t1"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'Ellis' )
  AND 
  "s_12_8_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
  AND 
  "s_12_8_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#homeTel' , 1))
OPTION (QUIETCAST)
