 SELECT  __ro2sq ("s_10_4_rbc"."property") AS "property" FROM (SELECT DISTINCT "s_10_4_t1"."P" AS "property"
  FROM DB.DBA.RDF_QUAD AS "s_10_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_10_4_t1"
    ON (
      "s_10_4_t0"."S" = "s_10_4_t1"."S")
  WHERE
    "s_10_4_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
    AND 
    "s_10_4_t0"."O" = __i2idn ( __bft( 'http://xmlns.com/foaf/0.1/Person' , 1))
OPTION (QUIETCAST)) AS "s_10_4_rbc"
