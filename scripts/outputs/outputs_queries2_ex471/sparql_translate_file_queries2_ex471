SELECT __ro2sq ( "s_13_4_t0"."O") AS "creator",
  __id2in ( "s_13_4_t1"."P") AS "propertyName",
  __ro2sq ( "s_13_4_t1"."O") AS "propertyValue"
FROM DB.DBA.RDF_QUAD AS "s_13_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_13_4_t1"
  ON (
    "s_13_4_t0"."O" = "s_13_4_t1"."S")
WHERE
  "s_13_4_t0"."P" = __i2idn ( __bft( 'http://purl.org/dc/elements/1.1/creator' , 1))
OPTION (QUIETCAST)
