SELECT  COUNT ( DISTINCT
     "s_37_7_t1"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_7_t1"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_12_t2"
  ON (
    "s_37_12_t2"."S" = "s_37_7_t1"."O")
  INNER JOIN (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_18_t3"."S" AS "_::trans_subj_37_14",
      "s_37_18_t3"."O" AS "_::trans_obj_37_15"
    FROM DB.DBA.RDF_QUAD AS "s_37_18_t3"
    WHERE
      "s_37_18_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_21"
  ON (
    "s_37_12_t2"."O" = "s_37_21"."_::trans_subj_37_14")
WHERE
  "s_37_7_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_12_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P31' , 1))
  AND 
  isiri_id ( "s_37_12_t2"."O")
  AND 
 ( "s_37_21"."_::trans_obj_37_15" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4671277' , 1)))
  AND 
  not ( EXISTS ( (
     SELECT TOP 1 1 AS __ask_retval
      FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
      WHERE
        "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P17' , 1))
        AND 
        "s_37_2_t0"."S" = "s_37_7_t1"."S"
OPTION (QUIETCAST)
     )))
  AND 
  1
OPTION (QUIETCAST)
