SELECT TOP 10 __bft( 'http://www.wikidata.org/entity/Q221226' , 1) AS "var1",
  NULL /* due to conflict on var2 */ AS "var2",
  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_3_t0"."O")) AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."P" = __i2idn ( __bft( 'http://schema.org/description' , 1))
  AND 
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q221226' , 1))
  AND 
  DB.DBA.RDF_LANGMATCHES ( __ro2sq ( "s_37_3_t0"."O"), 'ar' )
OPTION (QUIETCAST)
