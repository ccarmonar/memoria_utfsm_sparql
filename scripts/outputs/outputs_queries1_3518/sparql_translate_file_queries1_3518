SELECT  COUNT ( DISTINCT
     "s_37_20_t3"."S") AS "var2"
FROM DB.DBA.RDF_QUAD AS "s_37_20_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t0"
  ON (
    "s_37_20_t3"."S" = "s_37_5_t0"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t1"
  ON (
    "s_37_10_t1"."S" = "s_37_5_t0"."O")
  INNER JOIN (
   SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_16_t2"."S" AS "_::trans_subj_37_12",
      "s_37_16_t2"."O" AS "_::trans_obj_37_13"
    FROM DB.DBA.RDF_QUAD AS "s_37_16_t2"
    WHERE
      "s_37_16_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_19"
  ON (
    "s_37_10_t1"."O" = "s_37_19"."_::trans_subj_37_12")
WHERE
  "s_37_20_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2003' , 1))
  AND 
  "s_37_5_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_10_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P31' , 1))
  AND 
  isiri_id ( "s_37_10_t1"."O")
  AND 
 ( "s_37_19"."_::trans_obj_37_13" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4671277' , 1)))
  AND 
  1
OPTION (QUIETCAST)
