 SELECT  __ro2sq ("s_37_8_rbc"."var2") AS "var2", __ro2sq ("s_37_8_rbc"."var4") AS "var4" FROM (SELECT TOP 100 __id2in ( "s_37_8_t1"."S") AS "var2",
     DB.DBA.GROUP_CONCAT_DISTINCT (
       __id2in ( "s_37_8_t0"."S")) AS "var4"
  FROM DB.DBA.RDF_QUAD AS "s_37_8_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
    ON (
      "s_37_8_t0"."S" = "s_37_8_t1"."O")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t2"
    ON (
      "s_37_8_t1"."S" = "s_37_8_t2"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t3"
    ON (
      "s_37_8_t1"."S" = "s_37_8_t3"."S"
      AND 
      "s_37_8_t2"."S" = "s_37_8_t3"."S")
  WHERE
    "s_37_8_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_8_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q171' , 1))
    AND 
    "s_37_8_t1"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    "s_37_8_t2"."P" = __i2idn ( __bft( 'http://schema.org/inLanguage' , 1))
    AND 
    "s_37_8_t2"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'en' )
    AND 
    "s_37_8_t3"."P" = __i2idn ( __bft( 'http://schema.org/isPartOf' , 1))
    AND 
    "s_37_8_t3"."O" = __i2idn ( __bft( 'https://en.wikipedia.org/' , 1))
  GROUP BY "s_37_8_t1"."S"
OPTION (QUIETCAST)) AS "s_37_8_rbc"
