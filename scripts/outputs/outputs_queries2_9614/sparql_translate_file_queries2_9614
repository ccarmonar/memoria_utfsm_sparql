 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", "s_37_2_rbc"."var2Label" AS "var2Label" FROM (SELECT DISTINCT TOP 100 "s_37_2_t0"."S" AS "var1",
    NULL /* due to conflict on var2Label */ AS "var2Label"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q515' , 1))
    AND 
    rdf_contains_impl ( NULL /* due to conflict on var2Label */,  DB.DBA.RDF_MAKE_LONG_OF_SQLVAL ( 'San' ))
    AND 
    DB.DBA.RDF_LANGMATCHES ( NULL, 'en' )
OPTION (QUIETCAST)) AS "s_37_2_rbc"
