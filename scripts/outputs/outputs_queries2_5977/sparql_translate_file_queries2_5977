SELECT __bft( 'http://www.wikidata.org/entity/Q80863' , 1) AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
  ON (
    "s_37_8_t1"."S" = "s_37_4_t0"."O")
WHERE
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P31' , 1))
  AND 
  "s_37_8_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P31' , 1))
  AND 
  "s_37_8_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q11173' , 1))
  AND 
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q80863' , 1))
OPTION (QUIETCAST)
