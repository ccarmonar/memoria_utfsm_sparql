SELECT __bft( 'http://www.wikidata.org/entity/Q42' , 1) AS "var1",
  __id2in ( "s_37_3_t0"."P") AS "var2",
  NULL /* due to conflict on var3 */ AS "var3",
  NULL /* due to conflict on var4 */ AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_3_t0"
WHERE
  "s_37_3_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/statement/q42-BB4B67FE-FECA-4469-9DEE-3E8F03AC9F1D' , 1))
  AND 
  "s_37_3_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
OPTION (QUIETCAST)
