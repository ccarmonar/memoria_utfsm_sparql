SELECT __ro2sq ( "s_13_8_t3"."O") AS "first",
  __ro2sq ( "s_13_8_t4"."O") AS "last"
FROM DB.DBA.RDF_QUAD AS "s_13_8_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_13_8_t4"
  ON (
    "s_13_8_t4"."S" = "s_13_8_t3"."S")
  LEFT OUTER JOIN (
    SELECT "s_12_6_t0"."S" AS "s",
      "s_12_6_t0"."O" AS "workNum"
    FROM DB.DBA.RDF_QUAD AS "s_12_6_t0"
    WHERE
      "s_12_6_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#workTel' , 1))) AS "s_12_6"
  ON (
    "s_13_8_t4"."S" = "s_12_6"."s"
    AND 
    "s_13_8_t3"."S" = "s_12_6"."s")
WHERE
  "s_13_8_t3"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
  AND 
  "s_13_8_t4"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
  AND 
  not ( ( "s_12_6"."workNum" is not null))
OPTION (QUIETCAST)
