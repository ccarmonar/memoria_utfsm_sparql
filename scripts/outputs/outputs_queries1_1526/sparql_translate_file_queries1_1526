 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1", __ro2sq ("s_37_6_rbc"."var2") AS "var2" FROM (SELECT __id2in ( "s_37_5"."var1") AS "var1",
     COUNT (
       1) AS "var2"
  FROM (
     SELECT TOP 100000 "s_37_4_t0"."S" AS "var3",
        "s_37_4_t1"."P" AS "var1",
        "s_37_4_t1"."O" AS "var4"
      FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
        INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
        ON (
          "s_37_4_t0"."S" = "s_37_4_t1"."S")
      WHERE
        "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
        AND 
        "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4167836' , 1))
OPTION (QUIETCAST)
      ) AS "s_37_5"
  GROUP BY "s_37_5"."var1"
OPTION (QUIETCAST)) AS "s_37_6_rbc"
