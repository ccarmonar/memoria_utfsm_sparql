SELECT __bft( 'http://www.wikidata.org/entity/Q42' , 1) AS "var1",
  __id2in ( "s_37_5_t0"."P") AS "var2",
  __ro2sq ( "s_37_5_t1"."O") AS "var3",
  __ro2sq ( "s_37_5_t0"."O") AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_5_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t1"
  ON (
    "s_37_5_t0"."P" = "s_37_5_t1"."S")
WHERE
  "s_37_5_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
OPTION (QUIETCAST)
