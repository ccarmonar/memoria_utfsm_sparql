 DB.DBA.RDF_DICT_OF_TRIPLES_TO_THREE_COLS ((SELECT TOP 1  DB.DBA.SPARQL_CONSTRUCT (
      vector (
        vector (
         1,
         0,
         3,
         __i2id ( UNAME'http://xmlns.com/foaf/0.1/mbox' ),
         1,
         1)),
      vector (
       "s_9_5_t1"."S",
       __ro2lo ( "s_9_5_t1"."O")),
      vector (),
     1) AS "callret-0"
FROM DB.DBA.RDF_QUAD AS "s_9_5_t1"
WHERE
  "s_9_5_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#email' , 1))
OPTION (QUIETCAST)))
