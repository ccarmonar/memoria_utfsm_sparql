SELECT __id2in ( "s_37_2_t0"."P") AS "var1",
  __ro2sq ( "s_37_2_t0"."O") AS "var2",
  case when ( (1 - isiri_id ( "s_37_2_t0"."O"))) then ( rdf_concat_impl ( 'ss' ,  __rdf_strsqlval ( DB.DBA.RDF_DATATYPE_OF_OBJ (__ro2sq ( "s_37_2_t0"."O")), 0))) else ( '' ) end AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12418' , 1))
OPTION (QUIETCAST)
