SELECT 0 AS "var2",
  is_named_iri_id ( "s_37_5"."var3") AS "var4"
FROM (
   SELECT TOP 1000 "s_37_4_t0"."S" AS "var5",
      "s_37_4_t1"."O" AS "var3"
    FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
      ON (
        "s_37_4_t0"."S" = "s_37_4_t1"."S")
    WHERE
      "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
      AND 
      "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q1021645' , 1))
      AND 
      "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P17' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_5"
OPTION (QUIETCAST)
