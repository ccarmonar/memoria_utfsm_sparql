 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var3") AS "var3" FROM (SELECT __id2in ( "s_37_4_t0"."S") AS "var1",
     COUNT ( DISTINCT
       "s_37_4_t1"."S") AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."O")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P106' , 1))
    AND 
    "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4964182' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P50' , 1))
  GROUP BY "s_37_4_t0"."S"
  ORDER BY 2 DESC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
