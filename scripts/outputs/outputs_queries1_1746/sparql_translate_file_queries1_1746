SELECT TOP 200 __id2in ( "s_37_6_t0"."S") AS "var1",
  __id2in ( "s_37_6_t1"."P") AS "var2",
  __ro2sq ( "s_37_6_t1"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t0"."S" = "s_37_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
  ON (
    "s_37_6_t1"."P" = "s_37_6_t2"."O")
WHERE
  "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_6_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q4167836' , 1))
  AND 
  "s_37_6_t2"."P" = __i2idn ( __bft( 'http://wikiba.se/ontology#directClaim' , 1))
OPTION (QUIETCAST)
