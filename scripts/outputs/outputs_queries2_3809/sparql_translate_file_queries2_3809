SELECT TOP 10 __id2in ( "s_37_2_t0"."S") AS "var1"
FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
WHERE
  "s_37_2_t0"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
  AND 
  "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q52' , 1))
  AND 
  (cast ( __bft (__id2in ( "s_37_2_t0"."S"), 2) as varchar) like  '%string1%' )
OPTION (QUIETCAST)
