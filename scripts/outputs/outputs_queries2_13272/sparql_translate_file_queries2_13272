 SELECT  __ro2sq ("s_37_6_rbc"."var1") AS "var1" FROM (SELECT DISTINCT "s_37_6_t0"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t2"
    ON (
      "s_37_6_t0"."S" = "s_37_6_t2"."O"
      AND 
      "s_37_6_t1"."S" = "s_37_6_t2"."O")
  WHERE
    "s_37_6_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_6_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q12136' , 1))
    AND 
    "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P1995' , 1))
    AND 
    "s_37_6_t1"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q83042' , 1))
    AND 
    "s_37_6_t2"."P" = __i2idn ( __bft( 'http://schema.org/about' , 1))
    AND 
    (cast ( __bft (__id2in ( "s_37_6_t2"."S"), 2) as varchar) like  '%string1%' )
OPTION (QUIETCAST)) AS "s_37_6_rbc"
