SELECT __bft( 'http://www.wikidata.org/entity/Q586211' , 1) AS "var1",
  __id2in ( "s_37_5_t0"."P") AS "var2",
  __ro2sq ( "s_37_5_t0"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_5_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t1"
  ON (
    "s_37_5_t0"."P" = "s_37_5_t1"."S")
WHERE
  "s_37_5_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/1999/02/22-rdf-syntax-ns#type' , 1))
  AND 
  "s_37_5_t1"."O" = __i2idn ( __bft( 'http://www.w3.org/2002/07/owl#ObjectProperty' , 1))
  AND 
  "s_37_5_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q586211' , 1))
OPTION (QUIETCAST)
