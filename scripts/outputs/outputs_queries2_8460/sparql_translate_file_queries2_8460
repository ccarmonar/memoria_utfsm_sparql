SELECT __ro2sq ( "s_37_8_t1"."O") AS "var1",
  __ro2sq ( "s_37_8_t2"."O") AS "var2",
  __ro2sq ( "s_37_8_t3"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_8_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t1"
  ON (
    "s_37_8_t0"."O" = "s_37_8_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t2"
  ON (
    "s_37_8_t0"."O" = "s_37_8_t2"."S"
    AND 
    "s_37_8_t1"."S" = "s_37_8_t2"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_8_t3"
  ON (
    "s_37_8_t0"."O" = "s_37_8_t3"."S"
    AND 
    "s_37_8_t1"."S" = "s_37_8_t3"."S"
    AND 
    "s_37_8_t2"."S" = "s_37_8_t3"."S")
WHERE
  "s_37_8_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q656' , 1))
  AND 
  "s_37_8_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/P1448' , 1))
  AND 
  "s_37_8_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/statement/P1448' , 1))
  AND 
  "s_37_8_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/qualifier/P580' , 1))
  AND 
  "s_37_8_t3"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/qualifier/P582' , 1))
  AND 
 ( 'ru'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_8_t1"."O")))
OPTION (QUIETCAST)
