SELECT TOP 100 __id2in ( "s_37_4_t0"."S") AS "var1",
  __bft( 'http://www.wikidata.org/prop/direct/P36' , 1) AS "var2",
  __ro2sq ( "s_37_4_t0"."O") AS "var3",
  __ro2sq ( "s_37_4_t1"."O") AS "var4"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."S")
WHERE
  "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
  AND 
  "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P36' , 1))
OPTION (QUIETCAST)
