SELECT __id2in ( "s_37_4_t0"."S") AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2",
  __ro2sq ( "s_37_4_t1"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
  ON (
    "s_37_4_t0"."S" = "s_37_4_t1"."S"
    AND 
    "s_37_4_t0"."P" = "s_37_4_t1"."P")
WHERE
  "s_37_4_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( 'Sarawak' )
OPTION (QUIETCAST)
