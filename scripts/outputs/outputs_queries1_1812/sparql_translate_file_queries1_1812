SELECT __bft( 'http://www.wikidata.org/entity/Q188670' , 1) AS "var1",
  __id2in ( "s_37_4_t0"."P") AS "var2",
  NULL /* due to conflict on var2Label */ AS "var2Label",
  __bft( 'http://www.wikidata.org/entity/Q188993' , 1) AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
WHERE
  "s_37_4_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q188670' , 1))
  AND 
  "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q188993' , 1))
OPTION (QUIETCAST)
