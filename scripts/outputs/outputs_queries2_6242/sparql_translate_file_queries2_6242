 SELECT  __ro2sq ("s_37_14_rbc"."var1") AS "var1", __ro2sq ("s_37_14_rbc"."var2") AS "var2" FROM (SELECT DISTINCT "s_37_13_t2"."S" AS "var1",
    "s_37_13_t2"."O" AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_13_t1"
    INNER JOIN (
     SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_7_t0"."S" AS "_::trans_subj_37_2",
        "s_37_7_t0"."O" AS "_::trans_obj_37_3"
      FROM DB.DBA.RDF_QUAD AS "s_37_7_t0"
      WHERE
        ( "s_37_7_t0"."P" in ( __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1), 1))))
OPTION (QUIETCAST)
      ) AS "s_37_10"
    ON (
      "s_37_13_t1"."O" = "s_37_10"."_::trans_subj_37_2")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_13_t2"
    ON (
      "s_37_13_t2"."S" = "s_37_10"."_::trans_obj_37_3")
  WHERE
    "s_37_13_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    isiri_id ( "s_37_13_t1"."O")
    AND 
    ( "s_37_13_t2"."S" < min_bnode_iri_id ())
    AND 
    "s_37_13_t2"."P" = __i2idn ( __bft( 'http://www.w3.org/2000/01/rdf-schema#label' , 1))
    AND 
    "s_37_13_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q10547' , 1))
    AND 
   ( 'fa'  =  DB.DBA.RDF_LANGUAGE_OF_OBJ (__ro2sq ( "s_37_13_t2"."O")))
    AND 
    ( "s_37_13_t2"."S" in ( __i2idn ( __bft ( __bft( 'http://www.wikidata.org/entity/Q43229' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/entity/Q17334923' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/entity/Q215627' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/entity/Q2221906' , 1), 1))))
    AND 
    1
    AND 
    1
    AND 
   ( "s_37_13_t1"."O" =  "s_37_13_t1"."O")
OPTION (QUIETCAST)) AS "s_37_14_rbc"
