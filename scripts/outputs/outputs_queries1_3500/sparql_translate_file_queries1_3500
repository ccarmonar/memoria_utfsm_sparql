 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var2") AS "var2" FROM (SELECT __id2in ( "s_37_4_t0"."S") AS "var1",
    __ro2sq ( "s_37_4_t1"."O") AS "var2"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P21' , 1))
    AND 
    "s_37_4_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q6581072' , 1))
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P597' , 1))
    AND 
   ( 10 >  rdf_strlen_impl ( __ro2sq ( "s_37_4_t1"."O")))
  ORDER BY rdf_strlen_impl ( __ro2sq ( "s_37_4_t1"."O")) ASC
OPTION (QUIETCAST)) AS "s_37_4_rbc"
