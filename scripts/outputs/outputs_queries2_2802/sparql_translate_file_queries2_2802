 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", "s_37_2_rbc"."var2" AS "var2", "s_37_2_rbc"."var3" AS "var3" FROM (SELECT DISTINCT "s_37_2_t0"."S" AS "var1",
    NULL /* due to conflict on var2 */ AS "var2",
    NULL /* due to conflict on var3 */ AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q28220071' , 1))
OPTION (QUIETCAST)) AS "s_37_2_rbc"
