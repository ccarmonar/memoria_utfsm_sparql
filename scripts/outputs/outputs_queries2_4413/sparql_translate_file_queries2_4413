 SELECT  __ro2sq ("s_37_3_rbc"."var1") AS "var1" FROM (SELECT DISTINCT __id2in ( "s_37_2_t0"."S") AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q862597' , 1))
  ORDER BY __id2in ( "s_37_2_t0"."S") ASC
OPTION (QUIETCAST)) AS "s_37_3_rbc"
