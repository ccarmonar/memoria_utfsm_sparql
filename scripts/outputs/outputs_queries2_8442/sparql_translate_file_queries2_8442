 SELECT  __ro2sq ("s_37_11_rbc"."var1") AS "var1" FROM (SELECT DISTINCT TOP 500 "s_37_11_t1"."S" AS "var1"
  FROM DB.DBA.RDF_QUAD AS "s_37_11_t1"
    INNER JOIN (
     SELECT TRANSITIVE T_OUT(2) T_IN(1) T_MIN( 0) T_NO_CYCLES "s_37_7_t0"."S" AS "_::trans_subj_37_2",
        "s_37_7_t0"."O" AS "_::trans_obj_37_3"
      FROM DB.DBA.RDF_QUAD AS "s_37_7_t0"
      WHERE
        ( "s_37_7_t0"."P" in ( __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P131' , 1), 1)),  __i2idn ( __bft ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1), 1))))
OPTION (QUIETCAST)
      ) AS "s_37_10"
    ON (
      "s_37_11_t1"."O" = "s_37_10"."_::trans_subj_37_2")
  WHERE
    "s_37_11_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P31' , 1))
    AND 
    isiri_id ( "s_37_11_t1"."O")
    AND 
   ( "s_37_10"."_::trans_obj_37_3" =  __i2idn ( __bft( 'http://www.wikidata.org/entity/Q215627' , 1)))
    AND 
    1
OPTION (QUIETCAST)) AS "s_37_11_rbc"
