 SELECT  __ro2sq ("s_11_6_rbc"."first") AS "first", __ro2sq ("s_11_6_rbc"."last") AS "last" FROM (SELECT DISTINCT "s_11_6_t1"."O" AS "first",
    "s_11_6_t2"."O" AS "last"
  FROM DB.DBA.RDF_QUAD AS "s_11_6_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_11_6_t1"
    ON (
      "s_11_6_t0"."S" = "s_11_6_t1"."S")
    INNER JOIN DB.DBA.RDF_QUAD AS "s_11_6_t2"
    ON (
      "s_11_6_t0"."S" = "s_11_6_t2"."S"
      AND 
      "s_11_6_t1"."S" = "s_11_6_t2"."S")
  WHERE
    "s_11_6_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#takingCourse' , 1))
    AND 
    "s_11_6_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
    AND 
    "s_11_6_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
OPTION (QUIETCAST)) AS "s_11_6_rbc"
