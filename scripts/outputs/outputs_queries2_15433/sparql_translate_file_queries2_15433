 SELECT  __ro2sq ("s_37_4_rbc"."var1") AS "var1", __ro2sq ("s_37_4_rbc"."var2Label") AS "var2Label", __ro2sq ("s_37_4_rbc"."var3") AS "var3" FROM (SELECT DISTINCT "s_37_4_t0"."S" AS "var1",
    "s_37_4_t0"."P" AS "var2Label",
    "s_37_4_t1"."O" AS "var3"
  FROM DB.DBA.RDF_QUAD AS "s_37_4_t0"
    INNER JOIN DB.DBA.RDF_QUAD AS "s_37_4_t1"
    ON (
      "s_37_4_t0"."S" = "s_37_4_t1"."S")
  WHERE
    "s_37_4_t0"."O" = DB.DBA.RDF_MAKE_OBJ_OF_TYPEDSQLVAL ( '박원순' , __i2id( NULL),  'ko' )
    AND 
    "s_37_4_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P39' , 1))
OPTION (QUIETCAST)) AS "s_37_4_rbc"
