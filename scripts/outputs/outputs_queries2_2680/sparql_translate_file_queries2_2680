SELECT __id2in ( "s_37_16_t3"."S") AS "var1",
  __ro2sq ( "s_37_16_t3"."O") AS "var2",
  __ro2sq ( "s_37_14_t2"."O") AS "var3"
FROM DB.DBA.RDF_QUAD AS "s_37_16_t3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_5_t0"
  ON (
    "s_37_16_t3"."O" = "s_37_5_t0"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_10_t1"
  ON (
    "s_37_10_t1"."S" = "s_37_5_t0"."O")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_14_t2"
  ON (
    "s_37_14_t2"."S" = "s_37_10_t1"."O")
WHERE
  "s_37_5_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/ns/prov#wasDerivedFrom' , 1))
  AND 
  "s_37_10_t1"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/reference/P248' , 1))
  AND 
  "s_37_14_t2"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P356' , 1))
OPTION (QUIETCAST)
