 SELECT  __ro2sq ("s_37_2_rbc"."var1") AS "var1", __ro2sq ("s_37_2_rbc"."var1Label") AS "var1Label" FROM (SELECT TOP 1500, 500 __id2in ( "s_37_2_t0"."S") AS "var1",
    NULL /* due to conflict on var1Label */ AS "var1Label"
  FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
  WHERE
    "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P2180' , 1))
  GROUP BY "s_37_2_t0"."S", NULL /* due to conflict on var1Label */
  ORDER BY NULL /* due to conflict on var1Label */ ASC
OPTION (QUIETCAST)) AS "s_37_2_rbc"
