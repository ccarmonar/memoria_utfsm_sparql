SELECT __ro2sq ( "s_10_6_t1"."O") AS "first",
  __ro2sq ( "s_10_6_t2"."O") AS "last"
FROM DB.DBA.RDF_QUAD AS "s_10_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_10_6_t1"
  ON (
    "s_10_6_t0"."S" = "s_10_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_10_6_t2"
  ON (
    "s_10_6_t0"."S" = "s_10_6_t2"."S"
    AND 
    "s_10_6_t1"."S" = "s_10_6_t2"."S")
WHERE
  "s_10_6_t0"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#homeTel' , 1))
  AND 
  "s_10_6_t0"."O" = DB.DBA.RDF_OBJ_OF_SQLVAL ( '(229) 276-5135' )
  AND 
  "s_10_6_t1"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#firstName' , 1))
  AND 
  "s_10_6_t2"."P" = __i2idn ( __bft( 'http://learningsparql.com/ns/addressbook#lastName' , 1))
OPTION (QUIETCAST)
