SELECT __id2in ( "s_37_6_t1"."S") AS "var1",
  __ro2sq ( "s_37_6_t1"."O") AS "var2Label"
FROM (
   SELECT TOP 10000 "s_37_2_t0"."S" AS "var1"
    FROM DB.DBA.RDF_QUAD AS "s_37_2_t0"
    WHERE
      "s_37_2_t0"."P" = __i2idn ( __bft( 'http://www.wikidata.org/prop/direct/P279' , 1))
      AND 
      "s_37_2_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q35120' , 1))
OPTION (QUIETCAST)
    ) AS "s_37_3"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_6_t1"
  ON (
    "s_37_6_t1"."S" = "s_37_3"."var1")
WHERE
  "s_37_6_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2004/02/skos/core#altLabel' , 1))
  AND 
 ( 'en'  =  NULL)
OPTION (QUIETCAST)
