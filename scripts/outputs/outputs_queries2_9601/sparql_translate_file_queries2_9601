SELECT __bft( 'http://www.wikidata.org/entity/Q42' , 1) AS "var1",
  __id2in ( "s_37_7_t0"."P") AS "var2",
  __id2in ( "s_37_7_t1"."P") AS "var3",
  __ro2sq ( "s_37_7_t1"."O") AS "var4",
  __id2in ( "s_37_7_t2"."P") AS "var5",
  __ro2sq ( "s_37_7_t2"."O") AS "var6"
FROM DB.DBA.RDF_QUAD AS "s_37_7_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t1"
  ON (1)
  INNER JOIN DB.DBA.RDF_QUAD AS "s_37_7_t2"
  ON (
    "s_37_7_t1"."P" = "s_37_7_t2"."S")
WHERE
  "s_37_7_t0"."O" = __i2idn ( __bft( 'http://www.wikidata.org/entity/statement/q42-BB4B67FE-FECA-4469-9DEE-3E8F03AC9F1D' , 1))
  AND 
  "s_37_7_t1"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/statement/q42-BB4B67FE-FECA-4469-9DEE-3E8F03AC9F1D' , 1))
  AND 
  "s_37_7_t0"."S" = __i2idn ( __bft( 'http://www.wikidata.org/entity/Q42' , 1))
OPTION (QUIETCAST)
