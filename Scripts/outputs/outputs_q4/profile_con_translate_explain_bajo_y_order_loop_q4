SELECT __ro2sq ( "s_12_6_t0"."O") AS "givenName",
  __ro2sq ( "s_12_6_t1"."O") AS "familyName",
  __ro2sq ( "s_12_6_t2"."O") AS "hireDate"
FROM DB.DBA.RDF_QUAD AS "s_12_6_t0"
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_6_t1"
  ON (
    "s_12_6_t0"."S" = "s_12_6_t1"."S")
  INNER JOIN DB.DBA.RDF_QUAD AS "s_12_6_t2"
  ON (
    "s_12_6_t0"."S" = "s_12_6_t2"."S"
    AND 
    "s_12_6_t1"."S" = "s_12_6_t2"."S")
WHERE
  "s_12_6_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#given-name' , 1))
  AND 
  "s_12_6_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#family-name' , 1))
  AND 
  "s_12_6_t2"."P" = __i2idn ( __bft( 'http://www.snee.com/hr/hireDate' , 1))
  AND 
 ( "s_12_6_t2"."O" <  DB.DBA.RDF_OBJ_OF_SQLVAL ( '2015-03-01' ))
OPTION (QUIETCAST)

