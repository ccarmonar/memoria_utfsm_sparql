SELECT __ro2sq ( "s_9_5"."givenName") AS "givenName",
  __ro2sq ( "s_9_5"."familyName") AS "familyName",
  "s_9_5"."fullName" AS "fullName"
FROM (
   SELECT "s_9_4_t0"."S" AS "person",
      "s_9_4_t0"."O" AS "givenName",
      "s_9_4_t1"."O" AS "familyName",
      rdf_concat_impl ( __ro2sq ( "s_9_4_t0"."O"),  ' ' ,  __ro2sq ( "s_9_4_t1"."O")) AS "fullName"
    FROM DB.DBA.RDF_QUAD AS "s_9_4_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_9_4_t1"
      ON (
        "s_9_4_t0"."S" = "s_9_4_t1"."S")
    WHERE
      "s_9_4_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#given-name' , 1))
      AND 
      "s_9_4_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#family-name' , 1))
OPTION (QUIETCAST)
    ) AS "s_9_5"
OPTION (QUIETCAST)

