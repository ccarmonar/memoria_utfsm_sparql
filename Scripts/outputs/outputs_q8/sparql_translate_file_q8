SELECT __ro2sq ( "s_6_5"."givenName") AS "givenName",
  __ro2sq ( "s_6_5"."familyName") AS "familyName",
  'some value'  AS "someVariable"
FROM (
   SELECT "s_6_4_t0"."S" AS "person",
      "s_6_4_t0"."O" AS "givenName",
      "s_6_4_t1"."O" AS "familyName",
      'some value'  AS "someVariable"
    FROM DB.DBA.RDF_QUAD AS "s_6_4_t0"
      INNER JOIN DB.DBA.RDF_QUAD AS "s_6_4_t1"
      ON (
        "s_6_4_t0"."S" = "s_6_4_t1"."S")
    WHERE
      "s_6_4_t0"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#given-name' , 1))
      AND 
      "s_6_4_t1"."P" = __i2idn ( __bft( 'http://www.w3.org/2006/vcard/ns#family-name' , 1))
OPTION (QUIETCAST)
    ) AS "s_6_5"
OPTION (QUIETCAST)
